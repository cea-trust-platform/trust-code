# Hydraulique 2D turbulent K-Eps #
# PARALLEL OK 8 #
dimension 3
Pb_Hydraulique pb
Domaine dom
# BEGIN MESH #
Read_file dom model/grossier/Pb1.geom
dilate dom 1e-3
transformer dom x y z/50.
tetraedriser_homogene dom
# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partition_tool tranche { tranches 2 1 }
	Larg_joint 1
	zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFprep1b dis
Scheme_euler_implicit sch
Read sch
{
	tinit 0
	tmax 10.
	
dt_start dt_calc	
	
	dt_impr 0.1
	dt_sauv 1000.
	seuil_statio 1.e-8
dt_max 2e-3
facsec 1
	solveur implicite { solveur gmres { diag seuil 1e-18 impr nb_it_max 3 } }
}
Fluide_Incompressible fluide
Read fluide
{
	mu Champ_Uniforme 1 1e-3
	rho Champ_Uniforme 1 781
}
Associate pb dom
Associate pb sch
Associate pb fluide
Discretize pb dis

Read pb
{
	Navier_Stokes_standard
	{
		methode_calcul_pression_initiale avec_les_cl # pour forcer modele NS par defaut dans baltik ModeleCoeur #
		solveur_pression cholesky { }
solveur_pression petsc cholesky { }
		convection { Amont }
		diffusion { }
		initial_conditions {
			vitesse Champ_Uniforme 3 0. 0. 0.
			}
		boundary_conditions {
			 
			symetrie symetrie
			coude_defaut paroi_fixe
			in frontiere_ouverte_vitesse_imposee Champ_Front_Uniforme 3   0. -4 0
			r_1000_2000 frontiere_ouverte_pression_imposee  ch_front_input { nb_comp 1 nom Pression_chaude probleme pb }
		}	
	}
Post_processing 
{
	Definition_champs {
			vitesse_out Champ_post_interpolation {
                         localisation SOM
                        domaine dom_boundaries_r_1000_2000
                        source champ_post_refchamp
                          {
                            pb_champ  pb vitesse
                          }

                        }
	
	}
	Probes 
	{
		sonde_vitesse grav vitesse periode 1 segment 300 1.56 0. 0. 1.56 0.27  0.
		}
	format lata
	fields dt_post 0.1
	{
			pression_pa elem
			pression_pa som
			vitesse elem
			vitesse som
		
}	
}
Post_processings { 
	 bord {
	 format lata
	 domaine dom_boundaries_r_1000_2000 
	 fields dt_post 0.05
	 { 
	 pression_pa elem 
	vitesse som
	 }
	 }
	 }
}
# Solve pb #
End
End
