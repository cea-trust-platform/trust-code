   <dl_init>
   Memcheck:Leak
   match-leak-kinds: reachable
   fun:malloc
   obj:/usr/lib64/libstdc++.so.6.*
   fun:call_init.part.*
   fun:_dl_init
   obj:/usr/lib64/ld-*.so
   ...
}
