# Hydraulique 2D VEF : Bas Reynolds #
# PARALLEL OK 8 #
# Ne converge pas au meme temps car mp_min non fait pour le residu ? #
dimension 2

Pb_hydraulique pb
Domaine dom

# BEGIN MESH #
Mailler dom
{
	Pave Entree 
	{
	Origine 0. 0.
	Nombre_de_Noeuds 102 21
	Longueurs 5.05 1.
	}
	{
	Bord Entree X = 0.   0. <= Y <= 1.      
	Bord Haut   Y = 1.   0. <= X <= 5.05
	Bord Bas    Y = 0.   0. <= X <= 5.05   
	Bord Sortie X = 5.05 0. <= Y <= 1.
	}
}
trianguler_H dom
# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partition_tool metis { Nb_parts 2 }
	Larg_joint 2
	zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1B dis

Scheme_euler_explicit sch
Read sch
{
	tinit 0.
	tmax 1000.
	dt_min 0.0001
	dt_max 1.
	dt_impr 0.0001
	dt_sauv 1000
	seuil_statio 3.5
        diffusion_implicite 1
	seuil_diffusion_implicite 1.e-6
}

Fluide_Incompressible eau
Read eau
{
	mu Champ_Uniforme 1 2
	rho Champ_Uniforme 1 2
}

Associate pb dom
Associate pb sch
Associate pb eau

Discretize pb dis

Read pb
{
	Navier_Stokes_standard
	{
		# solveur_pression petsc GCP { precond ssor { omega 1.65 } seuil 1.e-7 impr } #
		solveur_pression petsc cli { -ksp_type cg -pc_type sor -pc_sor_omega 1.65 -mat_type aij -ksp_norm_type UNPRECONDITIONED -ksp_atol 1.e-7 impr }
		convection { amont }
		diffusion { }
		initial_conditions {
			vitesse Champ_Uniforme 2 0. 0.
			}
		boundary_conditions {
			Haut paroi_fixe
			Bas paroi_fixe
			Sortie frontiere_ouverte_pression_imposee Champ_Front_Uniforme 1 0.
			Entree frontiere_ouverte_vitesse_imposee Champ_Front_Uniforme 2  1. 0.			
		} 
		Parametre_equation parametre_diffusion_implicite
                {
			seuil_diffusion_implicite 1.e-14
                }
	}

	Post_processing
	{
		Probes
		{
			sonde_pression pression periode 0.0001 
				points 1 5. 0.5
			sonde_vitesse vitesse periode 0.0001
				segment 20 4. 0.025 4. 0.975
		}
		fields dt_post 0.5
		{
			pression elem
			pression som
			vitesse som
		}
	}
}

Solve pb
End

