# PARALLEL ONLY #
dimension 3
Pb_hydraulique pb
Domaine dom

Scatter DOM.Zones dom

VEFPreP1B dis

Scheme_euler_explicit sch
Read sch
{
    nb_pas_dt_max 3
    tinit 0.
    tmax 1.2
    dt_min 1e-10
    dt_max 1e10
    dt_impr 0.00001
    dt_sauv 100
    seuil_statio 1.e-8
}

Fluide_Incompressible fluide
Read fluide
{
    mu Champ_Uniforme 1 1.85e-5
    rho Champ_Uniforme 1 1.17
}
Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis

Read pb
{
    Navier_Stokes_Standard
    {
        solveur_pression GCP {
            precond ssor { omega 1.65 }
            seuil 1.e-17 impr
        }
        convection { amont }
        diffusion { }
        initial_conditions {
            vitesse champ_fonc_xyz dom 3 y*(0.01-y) 0. 0.
        }
        boundary_conditions {
            Haut 	paroi_fixe
            Bas 	paroi_fixe
            Gauche	frontiere_ouverte_vitesse_imposee champ_front_fonc_xyz 3 y*(0.01-y) 0. 0.
            Droit 	frontiere_ouverte_pression_imposee champ_front_uniforme 1 0
            Sud 	Symetrie
            Nord 	Symetrie
        }
    }
    Post_processing
    {
        format lata
        fields dt_post 0.1
        {
            vitesse elem
        }
    }
}

Solve pb
End

