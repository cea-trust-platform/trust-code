# Thermohydraulique 3D VDF couplee a conduction 2D avec rayonnement de paroi #
# PARALLEL OK 4 #
dimension 2    

Scheme_euler_implicit sch 
Read sch
{
        tinit 0.
        tmax 30
        dt_min 1.e-9
	dt_start dt_calc
	
        dt_impr 10.
        dt_sauv 500.
        seuil_statio 1.e-5
	diffusion_implicite 0
	facsec 200.
	solveur simpler
	{
		no_qdm
		seuil_convergence_solveur 1e-10
		seuil_convergence_implicite 1e-10
	}
}

Pb_conduction pb1
Pb_Thermohydraulique pb2

Domaine dom_solide
Domaine dom_fluide
# BEGIN MESH #
Mailler dom_fluide
{
        Pave Cavite
                {
                Origine 0. 0.
                Nombre_de_Noeuds 12 2
                Longueurs 0.05 0.05
                }
                {
                Raccord local homogene Gauche   X = 0.    0. <= Y <= 0.05 
                Bord Droit    X = 0.05  0. <= Y <= 0.05
                Bord Bas                        Y = 0.    0. <= X <= 0.05
                Bord Haut                       Y = 0.05  0. <= X <= 0.05
                }
}

Mailler dom_solide
{
        Pave Solide_Gauche
                {
                Origine -0.01 0.
                Nombre_de_Noeuds 4 2
                Longueurs 0.01 0.05
                }
                {
                Raccord local homogene Gauche    X = 0.       0. <= Y <= 0.05
                Bord Gauche_Gauche               X = -0.01    0. <= Y <= 0.05
                Bord Bas_Gauche                  Y = 0.    -0.01 <= X <= 0.  
                Bord Haut_Gauche                 Y = 0.05  -0.01 <= X <= 0.
                }
}



# END MESH #

# BEGIN PARTITION
Partition dom_solide
{
	Partitionneur metis { Nb_parts 2 }
	Larg_joint 1
	Nom_Zones DOM1
}



Partition dom_fluide
{
	Partitionneur metis { Nb_parts 2 }
	Larg_joint 1
	Nom_Zones DOM2
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM1.Zones dom_solide
Scatter DOM2.Zones dom_fluide
END SCATTER #

VDF dis

Fluide_Incompressible fluide
Read fluide
{
        mu Champ_Uniforme 1 0.001
        rho Champ_Uniforme 1 1.
        lambda Champ_Uniforme 1 1.
        Cp Champ_Uniforme 1 1000.
        beta_th Champ_Uniforme 1 0.
}

Solide sol
Read sol
{
 rho Champ_Uniforme 1 1.
        lambda Champ_Uniforme 1 10.
        Cp Champ_Uniforme 1 100.
}

Associate pb1 dom_solide
Associate pb1 sol

Associate pb2 dom_fluide
Associate pb2 fluide

Probleme_Couple pbc
Associate pbc pb1
Associate pbc pb2
Associate pbc sch
Discretize pbc dis


Read pb2
{
        Navier_Stokes_standard
        {
                
      solveur_pression gcp  { precond ssor { omega -2 } SEUIl 1e10 quiet }       
                convection { negligeable }
                diffusion { negligeable }
                
                conditions_initiales {
                        vitesse Champ_Uniforme 2 0. 0.
                        }
                boundary_conditions {
                        Droit paroi_fixe
                        Haut paroi_fixe
                        Gauche  paroi_fixe
                        Bas paroi_fixe
                }
	
        }
        Convection_Diffusion_Temperature
        {
                diffusion { }
                convection { negligeable }
                boundary_conditions 
                {	
		 	Droit Paroi_temperature_imposee
                              Champ_Front_uniforme 1 0.
                        Haut Paroi_adiabatique
                        Gauche paroi_contact pb1 Gauche 
                        Bas Paroi_ADIABATIQUE
                }
                conditions_initiales { Temperature champ_fonc_xyz dom_fluide 1 
(x>0)*(18.31179308*SIN(30.79992798*x-1.539996399)+20.00-400.*x)+(x<0)*(-18.31179308*COS(3.079992798*x+.3079992798e-1)+20.00-40.*x)  }
        }
        Postraitement
        {
               definition_champs {  
		  combinaison_champ_elem_dom_fluide Interpolation {
		      localisation elem
		      source Transformation {
		  	methode formule expression 1 temperature_natif_dom_fluide-((x>0)*(18.31179308*SIN(30.79992798*x-1.539996399)*EXP(-0.9486355639*t)+20.00-400.*x)+(x<0)*(-18.31179308*COS(3.079992798*x+.3079992798e-1)*EXP(-0.9486355639*t)+20.00-40.*x))
			source refChamp { Pb_champ pb2 temperature }
		  	}	
		  }
		}
                Champs dt_post 1.
                {
			combinaison_champ_elem_dom_fluide
                        temperature elem
			
                }
	  Sondes
                { 
		T_FLUIDE temperature periode 2.e-3 segment 10 2.5e-3 2.5e-2 4.75e-2 2.5e-2
		TEST1D_FLUIDE temperature periode 100. segment 5 5.e-3 5.e-3 5.e-3 4.5e-2
		erreur combinaison_champ_elem_dom_fluide periode 2.e-3 points 1 0.025 0.025
		T temperature periode 2.e-3 points 1 0.025 0.025
                }
        }
}
Read pb1
{
        Conduction
        {
                diffusion { }
                conditions_initiales { Temperature champ_fonc_xyz dom_fluide 1 
(x]0)*(18.31179308*SIN(30.79992798*x-1.539996399)+20.00-400.*x)+(x<0)*(-18.31179308*COS(3.079992798*x+.3079992798e-1)+20.00-40.*x)  }
                boundary_conditions {
                       
			Gauche Paroi_contact pb2 Gauche
                        Gauche_Gauche Paroi_flux_impose Champ_front_Uniforme 1 400.
                        Bas_Gauche paroi_adiabatique
			Haut_Gauche paroi_adiabatique
                }
        }
        Postraitement
        {
                Sondes
                {
                T_SOLIDE_GAUCHE temperature periode 2.e-3 segment 2 -7.5e-3 2.5e-2 -2.5e-3 2.5e-2
                T_SOLIDE_DROITE temperature periode 100. segment 2 5.25e-2 2.5e-2 5.75e-2 2.5e-2
		TEST1D_SOLGAUCH temperature periode 100. segment 2 -2.5e-3 5.e-3 -2.5e-3 4.5e-2
		TEST1D_SOLDROIT temperature periode 100. segment 2 5.25e-2 5.e-3 5.25e-2 4.5e-2
                }
                Champs dt_post 1.
                {
                        temperature elem
                }
        }
}

Solve pbc
End
