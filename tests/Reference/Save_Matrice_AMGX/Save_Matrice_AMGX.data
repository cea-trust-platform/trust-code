# Hydraulique 3D Force de pression: D=1m U=1 m/s Re=50 #
# PARALLEL NOT #

dimension 3

Pb_Hydraulique pb
Domaine dom

# BEGIN MESH #
Mailler dom
{
    Pave Cavite
    {
        Origine 		0. 0. 0.
        Nombre_de_Noeuds 	6 6 6
        Longueurs 		1 1 1
    }
    {
        Bord inlet   		X =  0.  0. <= Y <= 1.    0. <= Z <= 1.
        Bord outlet  		X =  1.  0. <= Y <= 1.    0. <= Z <= 1.
        Bord wall   		Y =  0.  0. <= X <= 1.    0. <= Z <= 1.
        Bord wall  		Y =  1.  0. <= X <= 1.    0. <= Z <= 1.
        Bord symmetry   	Z =  0.  0. <= X <= 1.    0. <= Y <= 1.
        Bord symmetry   	Z =  1.  0. <= X <= 1.    0. <= Y <= 1.
    }
}
Tetraedriser_homogene dom
Transformer dom x y*(1+0.1*x) z*(1+0.2*x*y)
# END MESH #

# BEGIN PARTITION
Partition dom
{
    Partition_tool metis { Nb_parts 2 }
    Larg_joint 2
    zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1B ma_discretisation

Schema_euler_implicite mon_schema
Read mon_schema
{
    nb_pas_dt_max 1
    tinit 0
    tmax 100.
    dt_min 1.e-6
    dt_max 100.
    dt_impr 1.e-6
    dt_sauv 100
    seuil_statio 1.e-3
    facsec 50
    facsec_max 300
    max_iter_implicite 50
    Solveur Implicite {
        solveur amgx gcp { precond gs { } seuil 1.e-8 impr save_matrix_mtx_format }
    }
}

Fluide_Incompressible milieu
Read milieu
{
    mu  Champ_Uniforme 1 0.04
    rho Champ_Uniforme 1 2.
}

Associate pb dom
Associate pb mon_schema
Associate pb milieu
Discretize pb ma_discretisation

Read pb
{
    Navier_Stokes_standard
    {
        solveur_pression amgx gcp { precond c-amg { } seuil 1.e-7 save_matrix_mtx_format }
        convection { muscl }
        diffusion { }
        initial_conditions {
            vitesse Champ_Uniforme 3 1. 0. 0.
        }
        boundary_conditions {
            wall paroi_fixe
            symmetry symetrie
            outlet frontiere_ouverte_pression_imposee Champ_front_Uniforme 1 0.
            inlet frontiere_ouverte_vitesse_imposee Champ_front_Uniforme 3 1. 0. 0.
        }
    }

    Post_processing
    {
        Probes
        {
            sonde_pression pression_pa periode 0.005 circle 11 0. 0. 0. 2 0.7 0. 360.
            sonde_vitesse vitesse periode 0.005 point 1 0.7 0. 0.
        }

        Format lml
        fields dt_post 1.
        {
            pression_pa elem
        }
    }
}

Solve pb
End
