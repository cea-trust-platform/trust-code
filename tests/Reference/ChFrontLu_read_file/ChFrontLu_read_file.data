# Lecture d'une condition limite dans un fichier .dat #
# PARALLEL OK #
dimension 3

Pb_Hydraulique_Turbulent pb

Domaine DOM_DOM

# PrecisionGeom est necessaire sur ce cas, ne pas l'utiliser systematiquement #
PrecisionGeom 1.e-8

# BEGIN MESH # 
Mailler DOM_DOM
{
Pave Cavite
        {
                Origine 0. 0. 0.
                Nombre_de_Noeuds 12 8 8
                Longueurs 6. 2. 2.
        }
        {
                Bord entree  	X = 0.    0. <= Y <= 2.     0. <= Z <= 2.
                Bord sortie  	X = 6.	  0. <= Y <= 2.     0. <= Z <= 2.
                Bord paroi     	Y = 0.    0. <= X <= 6.     0. <= Z <= 2.
                Bord paroi    	Y = 2.    0. <= X <= 6.     0. <= Z <= 2.
                Bord paroi     	Z = 0.    0. <= X <= 6.     0. <= Y <= 2.
                Bord paroi    	Z = 2.    0. <= X <= 6.     0. <= Y <= 2.
                }
}
Tetraedriser_homogene_fin DOM_DOM
# END MESH #

# BEGIN PARTITION
Partition DOM_DOM
{
	Partitionneur Tranche { Tranches 1 3 1 }
	Larg_joint 2
	Nom_Zones DOM
}
End 
END PARTITION #

# BEGIN SCATTER 
Scatter DOM.Zones DOM_DOM

END SCATTER #

VEFPreP1b dis

Scheme_euler_explicit sch
Read sch 
{
	nb_pas_dt_max 10
        tinit 0.
        tmax 1000.
        seuil_statio 1.e-9  
        facsec 1
	diffusion_implicite 0
}

Fluide_Incompressible fluide
Read fluide
{
	mu Champ_Uniforme	1 1.e-3
	rho Champ_Uniforme	1 1000
}

Associate pb DOM_DOM
Associate pb sch
Associate pb fluide

Discretize pb dis

Read pb
{
     Navier_Stokes_Turbulent
     {
	solveur_pression GCP { precond ssor { omega 1.4 } seuil 1.e-10 impr }
        convection { muscl }
        diffusion  { }
        conditions_initiales
                {
                vitesse Champ_Uniforme 3 0. 0. 0.
                }
        boundary_conditions
                {
		entree frontiere_ouverte_vitesse_imposee Champ_Front_lu DOM_DOM 3 pb_VITESSE_PERIO.dat 
	   	sortie frontiere_ouverte_pression_imposee Champ_Front_Uniforme 1 0. 
		paroi  paroi_fixe
                }
	Modele_turbulence K_Epsilon
        {
	  Transport_K_Epsilon
	  {
	    convection { amont }
	    diffusion  {   }
	    boundary_conditions
	    {
	      entree frontiere_ouverte_K_Eps_impose Champ_Front_lu DOM_DOM 2 pb_K_EPS_PERIO.dat
	      sortie frontiere_ouverte K_Eps_Ext Champ_Front_Uniforme 2 1.e-6 1.e-9
	      paroi paroi
	    }
	    conditions_initiales 
	    { 
	      K_Eps Champ_Uniforme 2 1.e-6 1.e-9
	    } 
	      
	  }
	  nut_max 0.01
	  turbulence_paroi loi_standard_hydr
	}
    }
    Postraitement
    {       
    	format lml
	Champs dt_post 100
      	{
      		vitesse elem 
      		K elem  
      	}
    }
}

Solve pb
End

