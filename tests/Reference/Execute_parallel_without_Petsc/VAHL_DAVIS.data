# Thermohydraulique 2D : Couplage Boussinesq et schema quick #
# PARALLEL OK 6 #
dimension 2
Pb_Thermohydraulique pb
Domaine dom
Domaine dom_lu
# BEGIN MESH #
Mailler dom
{
	Pave Cavite 
		{
		Origine 0. 0.
     		Nombre_de_Noeuds 11 11
		Longueurs 0.01 0.01
		}
		{
		Bord Gauche X = 0.  0. <= Y <= 0.01
		Bord Haut Y = 0.01  0. <= X <= 0.01
		Bord Bas Y = 0.     0. <= X <= 0.01
		Bord Droit X = 0.01 0. <= Y <= 0.01
		}
}
Ecrire_fichier_bin dom fichier_bin.geom
Read_file_binary dom_lu fichier_bin.geom
# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partitionneur trAnche {  tranchEs   2  	1 }
	Larg_joint 2
	Nom_Zones dom
}
End
END PARTITION #

# BEGIN SCATTER
Scatter dom.Zones dom
END SCATTER #

VDF dis

Scheme_euler_explicit sch
Read sch
{
	tinit 0.
	tmax 1.2
	dt_min 0.01
	dt_max 0.01
	dt_impr 0.01
	dt_sauv 0.1
	seuil_statio 1.e-8
	precision_impr 8
}

Fluide_Incompressible fluide
Read fluide
{
	mu Champ_Uniforme 1 1.85e-5
	rho Champ_Uniforme 1 1.17
	lambda Champ_Uniforme 1 0.0262 
	Cp Champ_Uniforme 1 1006
	beta_th Champ_Uniforme 1 3.41e-3
}

Champ_Uniforme gravite
Read gravite 2 0 -9.81

Associate fluide gravite
Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis

domaine front
extraire_surface 
  {
    domaine front
    probleme pb
    avec_certains_bords 1 Gauche  
  }
  
Read pb
{
	Navier_Stokes_standard
	{
		solveur_pression cholesky { }
		convection { quick }
		diffusion { }
		sources { boussinesq_temperature { T0 5. } }
		conditions_initiales {
			vitesse Champ_Uniforme 2 0. 0.
			}
		boundary_conditions {
			Haut paroi_fixe
			Bas paroi_fixe
			Gauche paroi_fixe
			Droit paroi_fixe
		}
	}
	Convection_Diffusion_Temperature
	{
		diffusion { }
		convection { quick }
		boundary_conditions 
		{
			Haut paroi_adiabatique
			Bas paroi_adiabatique
 			Gauche paroi_temperature_imposee Champ_Front_Uniforme 1 10. 
			Droit  paroi_temperature_imposee Champ_Front_Uniforme 1 0.  
		}
		conditions_initiales { Temperature Champ_Uniforme 1 5. }
	}

	Postraitements
	{		
	   post_dom {
	   fichier VAHL_DAVIS
		Definition_champs 
		{
		 pression_sonde_elem Interpolation {
		  	localisation elem
			source refChamp { Pb_champ pb pression }
		  	}
		 pression_som_dom Interpolation {
		  	localisation som
			source refChamp { Pb_champ pb pression }
		  	}
		 vitesse_som_dom Interpolation {
		  	localisation som
			source refChamp { Pb_champ pb vitesse }
		  	}
		 vitesse_transformation  Interpolation {
		       localisation elem
		       source Transformation {
		  	 methode formule expression 1 -1
			 source refChamp { Pb_champ pb vitesse }
		  	}
		   }
		 Moyenne_vitesse_temperature Moyenne {
		       t_deb 0.2 t_fin 0.7
		       source Transformation {
		  	methode formule expression 1 vitesse_natif_dom_ELEM_dom*temperature_natif_dom
		  	 sources
		  	 {
		  	   Interpolation { localisation elem source refChamp { Pb_champ pb vitesse } } ,
		  	   refChamp { Pb_champ pb temperature }
		  	 }
		       }
		   }
		  temperature_transformation Transformation {
		  	methode formule expression 1 temperature_natif_dom^2+100.*x
			source refChamp { Pb_champ pb temperature }
		  	}
			
		  vitesse_temperature Interpolation { localisation elem
		        source Transformation {
		  	 methode formule expression 1 1000.*vitesse_natif_dom*temperature_natif_dom
			 sources 
			  {
			   refChamp { Pb_champ pb temperature } ,
			   refChamp { Pb_champ pb vitesse }   
			  }
		  	}
		   }
		   
		  vitesse_vitesse Interpolation { localisation elem
		  	source Transformation {
		  	 methode formule expression 1 vitesse_natif_dom*vitesse_natif_dom
			 source refChamp { Pb_champ pb vitesse }
		  	 }
		  }
			 	
		  moyenne_t Moyenne
		  {
		    t_deb 0.2 t_fin 0.7
		    source refChamp { Pb_champ pb temperature }
		  }

		  moyenne_v_elem Moyenne
		  {
		    t_deb 0.2 t_fin 0.7
		    source Interpolation
		    {
		      localisation elem source refChamp { Pb_champ pb vitesse }
		    }
		  }

		  Correlation_vitesse_elem_temperature Correlation
		  {
		    t_deb 0.2 t_fin 0.7
		    sources
		    {
		      Interpolation
		      {
		        localisation elem source refChamp { Pb_champ pb vitesse }
		      } ,
		      refChamp { Pb_champ pb temperature }
		    }
		  }

		  convection_NS_dt Morceau_Equation { type operateur numero 1 option stabilite
			    	source refChamp { Pb_champ pb vitesse }
		   	}
		  convection_CDT_dt Morceau_Equation { type operateur numero 1 option stabilite source refChamp { Pb_champ pb temperature }
		   	}
		  mean_temperature1 Reduction_0D { methode moyenne source refChamp { Pb_champ pb temperature } }	
		  mean_temperature2 Reduction_0D { methode moyenne_ponderee source refChamp { Pb_champ pb temperature } }				
		}
		Sondes
		{
			temperature_transformation temperature_transformation periode 0.01 points 1 0.0045 0.0045
			sonde_V vitesse periode 0.01 segment 10 0.0005 0.005 0.0095 0.005
			sonde_pression pression periode 0.01 points 1 0.0045 0.0045
			sonde_vitesse vitesse periode 0.01 points 1 0.0045 0.0045
			sonde_temp temperature periode 0.01 points 1 0.0045 0.0045
			sonde_p pression periode 0.1 plan 10 10 0.0005 0.0005 0.0095 0.0005 0.0005 0.0095
			sonde_circle temperature periode 0.01 circle 20 0.005 0.005 0.0045 0. 360.
			sonde_vitesse_som  vitesse_som_dom  periode 0.01 points 1 0.0045 0.0045
			sonde_vitesse_som_0 vitesse_som_dom0 periode 0.01 points 1 0.0045 0.0045			
			sonde_V2 vitesse periode 0.01 segmentpoints 5 0.005 0.0075 3.01661665e-03 6.52190357e-03 0.0025 0.005 3.01661665e-03 3.47809643e-03 0.005 0.0025
			sonde_moyenne_temperature1 mean_temperature1 periode 0.01 numero_elem_sur_maitre 0
			sonde_moyenne_temperature2 mean_temperature2 periode 0.01 numero_elem_sur_maitre 0
		}	
		format lml
		Champs nb_pas_dt_post 10
		{
			pression elem
			pression_som_dom
			vitesse elem
			vitesse_som_dom
			temperature elem
			temperature som
			vorticite elem
			vitesse_transformation
			temperature_transformation
			convection_NS_dt
			convection_CDT_dt
			Moyenne_vitesse_temperature
			vitesse_temperature
			vitesse_vitesse
			Correlation_vitesse_elem_temperature
		}
	    }
	    
	  post_front
          { 
	   Definition_champs 
	    {
	     
             debit_fr Morceau_Equation {
	       type operateur numero 3 option flux_bords				   
	       source refChamp { Pb_champ pb vitesse } 
	      }	
	     
	     debit Interpolation {
						   domaine front 
						   localisation elem
		  			           source Morceau_Equation {
	                                            type operateur numero 3 option flux_bords				   
	                                            source refChamp { Pb_champ pb vitesse }
	       					   }
	      				         }

	    				       
	     diffU_X Interpolation {
						   domaine front 
						   localisation elem
		  			           source Morceau_Equation {
	                                            type operateur numero 0 option flux_bords compo 0				   
	                                            source refChamp { Pb_champ pb vitesse }
	       					   }
	      				       }
	     diffU_Y Interpolation {
						   domaine front 
						   localisation elem
		  			           source Morceau_Equation {
	                                            type operateur numero 0 option flux_bords compo 1				   
	                                            source refChamp { Pb_champ pb vitesse }
	       					   }
					      }
	     convU_X Interpolation {
						   domaine front 
						   localisation elem
		  			           source Morceau_Equation {
	                                            type operateur numero 1 option flux_bords compo 0				   
	                                            source refChamp { Pb_champ pb vitesse }
	       					   }
	      				       }
	     convU_Y Interpolation {
						   domaine front 
						   localisation elem
		  			           source Morceau_Equation {
	                                            type operateur numero 1 option flux_bords compo 1				   
	                                            source refChamp { Pb_champ pb vitesse }
	       					   }
	      				       }
					       
					       
	     diffT Interpolation {
						   domaine front 
						   localisation elem
		  			           source Morceau_Equation {
	                                            type operateur numero 0 option flux_bords				   
	                                            source refChamp { Pb_champ pb temperature }
	       					   }
	      				       }
					       			       			       
	     convT Interpolation {
						   domaine front 
						   localisation elem
		  			           source Morceau_Equation {
	                                            type operateur numero 1 option flux_bords			   
	                                            source refChamp { Pb_champ pb temperature }
	       					   }
	      				       } 			       
	     			       

	     
					       
	     debit_sum Reduction_0D { methode somme source_reference debit }				       
	     diffU_X_sum Reduction_0D { methode somme source_reference diffU_X }			       
             diffU_Y_sum Reduction_0D { methode somme source_reference diffU_Y }			       			       			       
	     convU_X_sum Reduction_0D { methode somme source_reference convU_X }			       
             convU_Y_sum Reduction_0D { methode somme source_reference convU_Y }
	     diffT_sum Reduction_0D { methode somme source_reference diffT }			       
             convT_sum Reduction_0D { methode somme source_reference convT }
	     			       			       
	    }
	    
	    Sondes
		{
			debit_gauche debit_sum periode 0.01 numero_elem_sur_maitre 0
			diffuX_gauche diffU_X_sum periode 0.01 numero_elem_sur_maitre 0
			diffuY_gauche diffU_Y_sum periode 0.01 numero_elem_sur_maitre 0
			convX_gauche convU_X_sum periode 0.01 numero_elem_sur_maitre 0
			convY_gauche convU_Y_sum periode 0.01 numero_elem_sur_maitre 0
			diffT_gauche diffT_sum periode 0.01 numero_elem_sur_maitre 0
			convT_gauche convT_sum periode 0.01 numero_elem_sur_maitre 0
		}
	   domaine front	
	   format lata
	   Champs dt_post 0.1
	    {
	     vitesse elem
             temperature elem 
	     debit_fr elem 
	     debit
	     diffU_X
	     diffU_Y
	     convU_X
	     convU_Y
	     diffT
	     convT
	     debit_sum
	     diffU_X_sum
	     diffU_Y_sum
	     convU_X_sum
	     convU_Y_sum
	     diffT_sum
	     convT_sum
	    }
	  }
	  
	}
	
	
	sauvegarde_simple formatte test_sauvegarde_simple.sauv	
}

Imprimer_flux dom { Gauche }
Solve pb
End
