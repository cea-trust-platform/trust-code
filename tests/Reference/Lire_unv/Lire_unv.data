# Cas pour tester la lecture d'un fichier Unv par ex venant d'Ideas #
dimension 3

Domaine dom


# BEGIN MESH #
lire_ideas dom ess.unv
# END MESH #

Pb_thermohydraulique_turbulent pb_plenum
VEFPreP1B dis

# Runge_Kutta_Rationnel_ordre_2 sch # 
Schema_Euler_explicite sch 
Lire sch 
{ 
        tinit 0.
        tmax 60.
        dt_min 1.e-8
        dt_max 1.e-1 
        dt_impr 1e-8
        dt_sauv 2.
        seuil_statio 1.e-8 
        facsec 1.
	nb_pas_dt_max 2
} 



Champ_Uniforme gravite
Lire gravite 3  0.  0. -9.81



Fluide_Incompressible helium_plenum
# Proprietes a T=809K calculees avec Sutherland et Godmann #
# beta=1/T #
Lire helium_plenum
{ 
        rho    Champ_Uniforme 1  3.37
        mu     Champ_Uniforme 1  3.93e-5
        lambda Champ_Uniforme 1  0.303
        Cp     Champ_Uniforme 1  5193
        beta_th Champ_Uniforme 1 1.24e-3
}



Associate helium_plenum gravite
Associate pb_plenum dom
Associate pb_plenum sch 
Associate pb_plenum helium_plenum

Discretize pb_plenum dis 

# Attention les conditions limites ne sont pas realistes #
Lire pb_plenum 
{ 
 Navier_Stokes_turbulent
 { 
     solveur_pression Gcp { precond ssor { omega 1.5 } 
                                             seuil 1.e-6 }
     convection { amont }
     diffusion  {  }

     Sources { Boussinesq_temperature { T0 936. } }


     conditions_initiales {  vitesse champ_uniforme  3 0. 0. 0.  }

     boundary_conditions { 
      paroi   paroi_fixe
      cl_IHX1 frontiere_ouverte_pression_imposee champ_front_uniforme 1 0.
      cl_IHX2 frontiere_ouverte_pression_imposee champ_front_uniforme 1 0.
      cl_IHX3 frontiere_ouverte_pression_imposee champ_front_uniforme 1 0.
      cl_DHR1 paroi_fixe
      cl_DHR2 paroi_fixe
      cl_DHR3 paroi_fixe
      cl_coeur frontiere_ouverte_vitesse_imposee champ_front_fonc_xyz 3 20. 10. 10. 
                        } 


    modele_turbulence K_epsilon
      {
        transport_k_epsilon 
          {
            convection { amont }
            diffusion { }
            conditions_initiales { k_eps Champ_uniforme 2 6. 900. }
            boundary_conditions {
                    paroi   paroi
                    cl_coeur frontiere_ouverte_K_eps_impose champ_front_fonc_xyz 2 6. 900.
		    cl_IHX1     frontiere_ouverte K_eps_ext   champ_front_uniforme 2 6. 900.
                    cl_IHX2     frontiere_ouverte K_eps_ext   champ_front_uniforme 2 6. 900.
                    cl_IHX3     frontiere_ouverte K_eps_ext   champ_front_uniforme 2 6. 900.
                    cl_DHR1     paroi
                    cl_DHR2     paroi
                    cl_DHR3     paroi
                               }
          }
        turbulence_paroi loi_standard_hydr nut_max 20. eps_min 1.e-9        
        }
 }

	Convection_Diffusion_Temperature_Turbulent
		{
		diffusion { }
		convection { amont }
		Modele_turbulence Prandtl
			{
			turbulence_paroi Loi_standard_hydr_scalaire
			}
		boundary_conditions 
			{
                        paroi  Paroi_temperature_imposee Champ_Front_Uniforme 1 673.
                        cl_IHX1    frontiere_ouverte T_ext Champ_Front_Uniforme 1 1000.             
                        cl_IHX2    frontiere_ouverte T_ext Champ_Front_Uniforme 1 1000.             
                        cl_IHX3    frontiere_ouverte T_ext Champ_Front_Uniforme 1 1000.             
                        cl_DHR1    Paroi_temperature_imposee Champ_Front_Uniforme 1 1000.             
                        cl_DHR2    Paroi_temperature_imposee Champ_Front_Uniforme 1 1000.             
                        cl_DHR3    Paroi_temperature_imposee Champ_Front_Uniforme 1 1000.             
                        cl_coeur  frontiere_ouverte_temperature_imposee champ_front_fonc_xyz 1 0.
		
			}
		conditions_initiales { Temperature Champ_Uniforme 1 1000. }
		}




  Postraitement
    {
     Sondes {

  	    point_vit0 vitesse periode 			1.e-3 point 1   0. 0.0  0.
  	    point_vit1 vitesse periode 			1.e-3 point 1   0. 0.0  0.4
  	    point_vit2 vitesse periode 			1.e-3 point 1   0. 0.0  3.
  	    point_vit3 vitesse periode 			1.e-3 point 1   0. 0.0  7.0
  	    point_vit4 vitesse periode 			1.e-3 point 1   0. 0.0  10.5

	    point_temperature0 temperature periode     	1.e-3 point 1   0. 0.0  0.
	    point_temperature1 temperature periode     	1.e-3 point 1   0. 0.0  0.4
	    point_temperature2 temperature periode     	1.e-3 point 1   0. 0.0  3.
	    point_temperature3 temperature periode     	1.e-3 point 1   0. 0.0  7.0
	    point_temperature4 temperature periode     	1.e-3 point 1   0. 0.0  10.5

	    point_nut0 viscosite_turbulente periode     1.e-3 point 1   0. 0.0  0.
	    point_nut1 viscosite_turbulente periode     1.e-3 point 1   0. 0.0  0.4
	    point_nut2 viscosite_turbulente periode     1.e-3 point 1   0. 0.0  3.
	    point_nut3 viscosite_turbulente periode     1.e-3 point 1   0. 0.0  7.
	    point_nut4 viscosite_turbulente periode     1.e-3 point 1   0. 0.0  10.5

	    point_k0 K periode     1.e-3 point 1   0. 0.0  0.
	    point_k1 K periode     1.e-3 point 1   0. 0.0  0.4
	    point_k2 K periode     1.e-3 point 1   0. 0.0  3.
	    point_k3 K periode     1.e-3 point 1   0. 0.0  7.
	    point_k4 K periode     1.e-3 point 1   0. 0.0  10.5

	    point_eps0 Eps periode     1.e-3 point 1   0. 0.0  0.
	    point_eps1 Eps periode     1.e-3 point 1   0. 0.0  0.4
	    point_eps2 Eps periode     1.e-3 point 1   0. 0.0  3.
	    point_eps3 Eps periode     1.e-3 point 1   0. 0.0  7.
	    point_eps4 Eps periode     1.e-3 point 1   0. 0.0  10.5

 


 }
     Champs dt_post 1.
            {
             pression elem
             vitesse elem
             temperature elem
             y_plus elem
             viscosite_turbulente elem
             K elem
             Eps elem

             }
     }
}

Solve pb_plenum
Fin

