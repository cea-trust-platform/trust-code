# Tests pour l'initialisation du canal pour n'importe quelle position dans l'espace #
# PARALLEL OK #

dimension 3

Domaine canal0
Domaine canal1
Domaine canal2

# BEGIN MESH #
Mailler canal0
        {
        Pave Cavite0
                {
                Origine -15. -1. 2.
                Nombre_de_Noeuds 9 40 9 
                Longueurs  10 2 2
                }
                {
                Bord periox0   X = -15.  -1. <= Y <= 1.    2. <= Z <= 4.
                Bord periox0   X = -5.   -1. <= Y <= 1.    2. <= Z <= 4.
                Bord perioz0   Z = 2.   -15. <= X <= -5   -1. <= Y <= 1.
                Bord perioz0   Z = 4.   -15. <= X <= -5.  -1. <= Y <= 1.
                Bord bas0      Y = -1.  -15. <= X <= -5.   2. <= Z <= 4.
                Bord haut0     Y = 1.   -15. <= X <= -5.   2. <= Z <= 4.
                }
        }
	
Mailler canal1
        {
        Pave Cavite1
                {
                Origine 2. 3. 5.
                Nombre_de_Noeuds 40 9 9 
                Longueurs  2 10 2
                }
                {
                Bord gauche1   X = 2.     3. <= Y <= 13.   5. <= Z <= 7.
                Bord droite1   X = 4.     3. <= Y <= 13.   5. <= Z <= 7.
                Bord perioz1   Z = 5.     2. <= X <= 4     3. <= Y <= 13.
                Bord perioz1   Z = 7.     2. <= X <= 4.    3. <= Y <= 13.
                Bord perioy1   Y = 3.     2. <= X <= 4.    5. <= Z <= 7.
                Bord perioy1   Y = 13.    2. <= X <= 4.    5. <= Z <= 7.
                }
        }
	
Mailler canal2
        {
        Pave Cavite2
                {
                Origine 5. -5. -10.
                Nombre_de_Noeuds 9 50 9 
                Longueurs  2 2 10
                }
                {
                Bord periox2   X = 5.   -5. <= Y <= -3.  -10. <= Z <= 0.
                Bord periox2   X = 7.   -5. <= Y <= -3.  -10. <= Z <= 0.
                Bord perioz2   Z = -10.  5. <= X <= 7     -5. <= Y <= -3.
                Bord perioz2   Z = 0.    5. <= X <= 7.    -5. <= Y <= -3.
                Bord bas2      Y = -3.   5. <= X <= 7.   -10. <= Z <= 0.
                Bord haut2     Y = -5.   5. <= X <= 7.   -10. <= Z <= 0.
                }
        }
# END MESH #
# BEGIN PARTITION
Partition canal0
{
	Tranche 2 1 1
	Nb_parts 2
	Larg_joint 1
	Nom_Zones DOM0
        PERIODIQUE_1PROC 2 periox0 perioz0 0 2
}
Partition canal1
{
	Tranche 1 2 1
	Nb_parts 2
	Larg_joint 1
	Nom_Zones DOM1
        PERIODIQUE_1PROC 2 perioy1 perioz1 1 2
}
Partition canal2
{
	Tranche 1 1 2
	Nb_parts 2
	Larg_joint 1
	Nom_Zones DOM2
        PERIODIQUE_1PROC 2 periox2 perioz2 0 2
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM0.Zones canal0
Scatter DOM1.Zones canal1
Scatter DOM2.Zones canal2
END SCATTER #

VDF dis


Runge_Kutta_ordre_3 sch
Lire sch
	{
	tinit 0.
        tmax 50.
	dt_min 1.e-7
	dt_max 1.e-1
	dt_impr 1e-5
	dt_sauv 500.
        seuil_statio 1.e-8
	nb_pas_dt_max 5
	}

Fluide_Incompressible fluide1
Lire fluide1
{
	mu Champ_Uniforme	1 1.e-5
	rho Champ_Uniforme	1 1.
}
Fluide_Incompressible fluide2
Lire fluide2
{
	mu Champ_Uniforme	1 1.e-5
	rho Champ_Uniforme	1 1.
}
Fluide_Incompressible fluide3
Lire fluide3
{
	mu Champ_Uniforme	1 1.e-5
	rho Champ_Uniforme	1 1.
}


Pb_Hydraulique_Turbulent pb1
Pb_Hydraulique_Turbulent pb2
Pb_Hydraulique_Turbulent pb3

Associate pb1 canal0
Associate pb2 canal1
Associate pb3 canal2

Associate pb1 fluide1
Associate pb2 fluide2
Associate pb3 fluide3

Probleme_Couple pb

Associate pb pb1
Associate pb pb2
Associate pb pb3

Associate pb sch


Discretize pb dis

Lire pb1
	{
	Navier_Stokes_Turbulent
		{
		solveur_pression GCP
			{
			precond ssor { omega 1.5 } 
			seuil 1e-8
			impr
			}
		convection { centre }
		diffusion { }
		Sources
			{
			Canal_perio { bord periox0 }
			}
		conditions_initiales
			{
			vitesse champ_init_canal_sinal 3 
				{
				Ucent 3.5
				h 1.0 
				ampli_sin 0.5
				omega 2.
				ampli_bruit 0.
				dir_flow 0
				dir_wall 1
				min_dir_flow -15
				min_dir_wall -1
				}
			}
		boundary_conditions
			{
			periox0   periodique 
			perioz0   periodique 
			haut0     paroi_fixe
			bas0      paroi_fixe
			}
		Modele_turbulence sous_maille_wale
			{
			turbulence_paroi negligeable
			cw 0.5
			}
		}

	Postraitement
        	{       
                Sondes
                	{ 
                        sonde_vit  vitesse periode 0.1 segment 65	-10. -1. 3.
									-10.  1. 3.
			}
			format lml
		Champs dt_post 0.5
			{
			vitesse elem
			}
		Statistiques dt_post 0.5
		{
			t_deb 0. t_fin 2.
                       	moyenne pression
                       	moyenne vitesse
                        ecart_type vitesse
                        ecart_type pression 				
		}			
		}
Sauvegarde formatte pb1.sauv

	}


Lire pb2
	{
	Navier_Stokes_Turbulent
		{
		solveur_pression GCP
			{
			precond ssor { omega 1.5 } 
			seuil 1e-7
			impr
			}
		convection { centre }
		diffusion { }
		Sources
			{
			Canal_perio { bord perioy1 }
			}
		conditions_initiales
			{
			vitesse champ_init_canal_sinal 3 
				{
				Ucent 7.
				h 1.0
				ampli_sin 1.
				omega 2.
				ampli_bruit 0.
				dir_flow 1
				dir_wall 0
				min_dir_flow 3.
				min_dir_wall 2.
				}
			}
		boundary_conditions
			{
			perioy1   periodique 
			perioz1   periodique 
			gauche1   paroi_fixe
		        droite1   paroi_fixe
			}
		Modele_turbulence sous_maille_wale
			{
			turbulence_paroi negligeable
			cw 0.5
			}
		}


	Postraitement
        	{       
                Sondes
                	{ 
                        sonde_vit_2  vitesse periode 0.1 segment 65	2. 8. 6
									4. 8. 6.
			}
			format lml
		Champs dt_post 0.5
			{
			vitesse elem
			}
		Statistiques dt_post 0.5
		{
			t_deb 0. t_fin 2.
                       	moyenne pression
                       	moyenne vitesse
                        ecart_type vitesse
                        ecart_type pression 				
		}			
		}
Sauvegarde formatte pb2.sauv

	}



Lire pb3
	{
	Navier_Stokes_Turbulent
		{
		solveur_pression GCP
			{
			precond ssor { omega 1.5 } 
			seuil 1e-7
			impr
			}
		convection { centre }
		diffusion { }
		Sources
			{
			Canal_perio { bord periox2 }
			}
		conditions_initiales
			{
			vitesse champ_init_canal_sinal 3 
				{
				Ucent 1.
				h 1.0
				ampli_sin 0.2
				omega 2.
				ampli_bruit 0.
				dir_flow 2
				dir_wall 1
				min_dir_flow -10.
				min_dir_wall -5.
				}
			}
		boundary_conditions
			{
			periox2   periodique 
			perioz2   periodique 
			bas2   paroi_fixe
		        haut2   paroi_fixe
			}
		Modele_turbulence sous_maille_wale
			{
			turbulence_paroi negligeable
			cw 0.5
			}
		}

	Postraitement
        {       
                Sondes
                	{ 
                        sonde_vit_avant  vitesse periode 0.1 segment 65	      6. -5. -5
									      6. -3. -5
			}
		format lml
		Champs dt_post 0.5
			{
			vitesse elem
			}
		Statistiques dt_post 0.5
		{
			t_deb 0. t_fin 2.
                       	moyenne pression
                       	moyenne vitesse
                        ecart_type vitesse
                        ecart_type pression 				
		}
	}
Sauvegarde formatte pb3.sauv
	}

Solve pb
Fin
