# SIMULATION D'UN CANAL PLAN 3D VEF EN THERMOHYDRAULIQUE INCOMPRESSIBLE #
# PARALLEL OK #
dimension 3

Pb_Thermohydraulique_turbulent_QC pb

Domaine dom

# BEGIN MESH #
Lire_Tgrid dom Canal_plan_bi_periodique_M1.msh
Transformer dom
 1*x
 y
 z
Corriger_frontiere_periodique { Domaine dom Bord Periox }
Corriger_frontiere_periodique { Domaine dom Bord Perioz }
# END MESH #

# BEGIN PARTITION
Partition dom
{
        Partitionneur tranche { tranches 1 2 1 }
        Larg_joint 2
        Nom_Zones DOM
        Periodique 2 Periox Perioz
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1B dis

Schema_Euler_explicite sch
Lire sch
{
	nb_pas_dt_max 20
	tinit	0.
	tmax	0.05
	dt_min	1.e-7
	dt_max	1.e-1
	dt_impr	1.e-5
	dt_sauv	0.01
        seuil_statio 1.e-8
}

Fluide_Quasi_Compressible fluide
Lire fluide
{
	mu	Champ_Fonc_fonction	1 temperature	3.95e-7*val^0.687
	lambda	Champ_Fonc_fonction	1 temperature	2.774e-3*val^0.701
        pression   7092750.
	loi_etat gaz_parfait
	{
                Prandtl   0.673
                Cp        5193.
                gamma     1.666
	}
	Traitement_Pth conservation_masse                                                                                                                                            	 
}

Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis
# Debog pb seq faces 1.e-4 0 #
Lire pb
{
	Navier_Stokes_turbulent_QC
	{
		solveur_pression GCP { precond ssor { omega 1.651 } seuil 1.e-10 }
		convection { muscl }
		diffusion { }
		Sources { Canal_perio { bord Periox } }
		conditions_initiales { vitesse champ_uniforme 3 80. 0. 0. }
		boundary_conditions
		{
			Periox	periodique 
			Perioz	periodique 
			Bas	Paroi_fixe
			Haut	Paroi_fixe
		}
                Modele_turbulence K_Epsilon
		{
			Transport_K_Epsilon 
			{
				convection { amont }
                                diffusion { }
                                boundary_conditions
				{
					Periox	periodique 
					Perioz	periodique 
					Bas	Paroi
					Haut	Paroi
				}
                                conditions_initiales { k_Eps Champ_Uniforme 2 64. 1.4e6 }
			}
			turbulence_paroi loi_standard_hydr dt_impr_ustar 0.001
		}
	}

	Convection_Diffusion_Chaleur_Turbulent_QC
	{
		diffusion { }
		convection { muscl }
		boundary_conditions 
		{
			Periox	periodique 
			Perioz	periodique 
			Bas	paroi_temperature_imposee Champ_Front_Uniforme 1 673.
			Haut	paroi_temperature_imposee Champ_Front_Uniforme 1 1073.
		}
		conditions_initiales { Temperature Champ_fonc_xyz dom 1 673. }
		Modele_turbulence Prandtl
		{   
                        Turbulence_paroi loi_standard_hydr_scalaire  dt_impr_nusselt 0.001                
 		}
	}

	Postraitement
        {       
		Sondes
                {
		  sonde_pression		pression		periode 0.0001 segment 50  0.00025 0.0 0.00025	0.00025 0.004 0.00025
		  sonde_vitesse		nodes	vitesse			periode 0.0001 segment 50  0.00025 0.0 0.00025	0.00025 0.004 0.00025
		  sonde_temperature	nodes	temperature		periode 0.0001 segment 50  0.00025 0.0 0.00025	0.00025 0.004 0.00025
		  sonde_rho		nodes	masse_volumique		periode 0.0001 segment 50  0.00025 0.0 0.00025	0.00025 0.004 0.00025
		  sonde_mu		nodes	viscosite_dynamique	periode 0.0001 segment 50  0.00025 0.0 0.00025	0.00025 0.004 0.00025
		  sonde_y_plus		nodes	y_plus			periode 0.0001 segment 50  0.00025 0.0 0.00025	0.00025 0.004 0.00025
		  sonde_k		nodes	k			periode 0.0001 segment 50  0.00025 0.0 0.00025	0.00025 0.004 0.00025
		  sonde_eps		nodes	eps			periode 0.0001 segment 50  0.00025 0.0 0.00025	0.00025 0.004 0.00025
		  sonde_visco_turb	nodes	viscosite_turbulente	periode 0.0001 segment 50  0.00025 0.0 0.00025	0.00025 0.004 0.00025
		}
                Champs dt_post 0.01
		{
		  pression		elem
		  vitesse		elem
		  temperature		elem
		  viscosite_turbulente	elem
		}
	}
}

Solve pb 
Fin


