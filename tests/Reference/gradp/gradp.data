# Hydraulique 2D : Condition de sortie libre gradP=0 #
# PARALLEL OK 8 #
dimension 2

Pb_hydraulique pb
Domaine dom
# BEGIN MESH #
Mailler dom 
{
	Pave Entree
		{
		Origine 0. 0.
		Nombre_de_Noeuds 6 23
		Longueurs 0.1 0.22
		}
		{
		bord Entree X = 0. 0. <= Y <= 0.22 
		bord Haut1 Y = 0.22 0. <= X <= 0.1 
		bord Bas1 Y = 0. 0. <= X <= 0.1 
		bord Droit X = 0.1 0.1 <= Y <= 0.12
		} ,

	Pave Haut
		{
		Origine 0.1 0.12
		Nombre_de_Noeuds 2 11
		Longueurs 0.02 0.1
		}
		{
		bord Haut2 Y = 0.22 0.1 <= X <= 0.12 
		bord Bas2 Y = 0.12 0.1 <= X <= 0.12
		} ,

	Pave Bas
		{
		Origine 0.1 0.
		Nombre_de_Noeuds 2 11
		Longueurs 0.02 0.1
		}
		{
		bord Bas3 Y = 0. 0.1 <= X <= 0.12 
		bord Haut3 Y = 0.1 0.1 <= X <= 0.12
		} ,

	Pave Sortie
		{
		Origine 0.12 0.
		Nombre_de_Noeuds 41 23
		Longueurs 0.8 0.22
		}
		{
		bord Sortie X = 0.92 0. <= Y <= 0.22 
		bord Bas4 Y = 0. 0.12 <= X <= 0.92 
		bord Haut4 Y = 0.22 0.12 <= X <= 0.92 
		bord Gauche X = 0.12 0.1 <= Y <= 0.12
		}
}
ecrire dom

# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 1
	Nom_Zones DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VDF dis

Scheme_euler_explicit sch
Read sch
{
	tinit 0
	tmax 5.0
	dt_min 0.0001
	dt_max 0.01
	dt_impr 0.0001
	dt_sauv 100
	seuil_statio 1.e-8
}

Fluide_Incompressible eau
Read eau
{
	mu Champ_Uniforme 1 3.7e-05
	rho Champ_Uniforme 1 2
}

Champ_Uniforme gravite
Read gravite 2 0 0
Associate pb dom
Associate pb sch
Associate pb eau
Discretize pb dis
Read pb
{
	Navier_Stokes_standard
	{
		solveur_pression GCP { 
			precond ssor { omega 1.500000 } 
			seuil 1.000000e-12
			impr 
		}
		convection { amont }
		diffusion { }
		conditions_initiales {
			vitesse Champ_Uniforme 2 0. 0.
			}
		boundary_conditions {
			Haut3 paroi_fixe
			Bas2 paroi_fixe
			Droit paroi_fixe
			Gauche paroi_fixe
			Haut1 symetrie
			Bas1  symetrie
			Haut2 symetrie
			Bas3  symetrie
		      	Bas4  symetrie
			Haut4 symetrie
			Sortie frontiere_ouverte_gradient_pression_impose Champ_Front_Uniforme 1 -1.
			Entree frontiere_ouverte_pression_imposee Champ_Front_Uniforme 1  0. 
		}
	}
	Postraitement
	{
		Sondes
		{
		 sonde_P pression periode 0.005 segment 46 0.01 0.105 0.91 0.105
		 sonde_pression pression periode 0.005 points 2 0.13 0.105 0.13 0.115
		 sonde_vitesse vitesse periode 0.005 points 2 0.14 0.105 0.14 0.115
		}
		Champs dt_post 3.
		{
			pression elem
			pression som
			vitesse elem
			vitesse som
		}
	}
}
Solve pb
End
End
