# Thermohydraulique 2D VEF : Conditions limites instationnaires FONC(T,X,Y,Z) #
# PARALLEL OK 8 #
dimension 2
Pb_hydraulique pb
Domaine dom
# BEGIN MESH #
Mailler dom
{
        Pave Cavite 
                {
                Origine 0. 0.
                Nombre_de_Noeuds 11 11
                Longueurs 1. 1.
                }
                {
                Bord Gauche X = 0.  0. <= Y <= 1.
                Bord Haut   Y = 1.  0. <= X <= 1.
                Bord Bas    Y = 0.  0. <= X <= 1.
                Bord Droit  X = 1.  0. <= Y <= 1.
                }
}
Trianguler_H dom
# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 2
	Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1B dis


Schema_Euler_explicite sch
Lire sch
{
        tinit 0.
        tmax 0.2
        dt_min 0.01
        dt_max 0.01
        dt_impr 0.001
        dt_sauv 0.01
        seuil_statio -1.e-8
}

Fluide_Incompressible fluide
Lire fluide
{
        mu Champ_Uniforme 1 1.85e-5
        rho Champ_Uniforme 1 1.17
}



Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis

Lire pb
{
        Navier_Stokes_standard
        {
                solveur_pression GCP { precond ssor { omega 1.65 } seuil 1.e-12 } 
                convection { amont }
                diffusion { }
                conditions_initiales {
                        vitesse Champ_Uniforme 2 0. 0.
                        }
                boundary_conditions {
                        Bas frontiere_ouverte_vitesse_imposee Champ_Front_fonc_txyz 2 0. SIN(10.*t)*x*(1-x)
                        Haut frontiere_ouverte_pression_imposee champ_front_uniforme 1 0.
                        Gauche paroi_fixe
                        Droit paroi_fixe
                }
        }
        Postraitement
        {
                Sondes
                {
                        sonde_pres   pression periode 0.01 points 1 0.55  0.
                        sonde_vit     vitesse periode 0.01 points 1 0.55  0.
			sonde_divergence divergence_U_som_dom periode 0.01 points 1 0.55  0.
                }
		
		Definition_champs
		{
		 divergence_U_som_dom  Interpolation {
		 			localisation som
		 			source Divergence {
		 		         source refChamp { Pb_champ pb vitesse } }
		  }
		  
		 divergence_U_elem_dom  Interpolation {
		 			localisation elem
		 			source Divergence {
		 		         source refChamp { Pb_champ pb vitesse } }
		  } 
		}
		
                Champs dt_post 100. 
                {
                        pression
                        vitesse
                }
        }
}

Solve pb
Fin
