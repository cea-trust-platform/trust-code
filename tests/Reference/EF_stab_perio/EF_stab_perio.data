# Hydraulique 2D VEF : Schema EF_stab, Resultats pas identiques en // #
# PARALLEL OK #
dimension 2
Pb_hydraulique pb
Domaine dom
# BEGIN MESH #
Mailler dom
{
    Pave Cavite
    {
        Origine 0. 0.
        Nombre_de_Noeuds 11 10
        Longueurs 1. 1.
    }
    {
        Bord Gauche X = 0.	0. <= Y <= 1.
        Bord perioy Y = 1.	0. <= X <= 1.
        Bord perioy Y = 0.	0. <= X <= 1.
        Bord Droit  X = 1.	0. <= Y <= 1.
    }
}
Trianguler_H dom
# END MESH #

# BEGIN PARTITION
Partition dom
{
    Partition_tool tranche { tranches 2 1 }
    Larg_joint 2
    zones_name DOM
    Periodique 1 perioy
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1B dis

Scheme_euler_explicit sch
Read sch
{
    nb_pas_dt_max 4
    tinit 0.
    tmax 1.2
    dt_min 0.00001
    dt_max 0.01
    dt_impr 0.0001
    dt_sauv 100
    seuil_statio 1.e-8
}

Fluide_Incompressible fluide
Read fluide
{
    mu  Champ_Uniforme 1 1.85e-5
    rho Champ_Uniforme 1 1.17
}

Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis

# DEBOG #
# Debog pb seq faces 1.e-6 0 #

Read pb
{
    Navier_Stokes_standard
    {
        solveur_pression GCP
        {
            precond ssor { omega 1.5 }
            seuil 1.e-9
            impr
        }
        convection { EF_stab { } }
        diffusion { }
        initial_conditions { vitesse Champ_Uniforme 2 0. 0. }
        boundary_conditions
        {
            perioy  periodique
            Gauche	frontiere_ouverte_vitesse_imposee  Champ_front_Uniforme 2 1. 0.
            Droit	frontiere_ouverte_pression_imposee Champ_front_Uniforme 1 0.
        }
    }

    Post_processing
    {
        Probes
        {
            sonde_pression	pression periode 0.01 points   1   0.45 0.45
            sonde_vitesse	vitesse  periode 0.01 segment 10   0.05 0.5	0.95 0.5
        }
        fields dt_post 10.
        {
            pression elem
            pression som
            vitesse som
        }
    }
}
Solve pb
End
