# Convergence study #
# PARALLEL OK #
dimension 2
Pb_hydraulique pb
Domaine dom

# Read the mesh #
# DEBUT MAILLAGE #
Mailler dom
{
	Pave Cavite 
		{
		Origine -0.5 -0.5
     		Nombre_de_Noeuds 33 33
		Longueurs 1.0 1.0
		}
		{
		Bord BOUNDARY 	X = -0.5  -0.5 <= Y <= 0.5
		Bord BOUNDARY 	Y = 0.5   -0.5 <= X <= 0.5
		Bord BOUNDARY	Y = -0.5  -0.5 <= X <= 0.5
		Bord BOUNDARY   X = 0.5   -0.5 <= Y <= 0.5
		}
}
dilate dom 0.5
Trianguler_H dom
# FIN MAILLAGE #

# DEBUT DECOUPAGE
Partition dom
{
	Partitionneur metis { Nb_parts 2 }
	Larg_joint 2
	Nom_Zones DOM
}
Fin
FIN DECOUPAGE #

# DEBUT LECTURE
Scatter DOM.Zones dom
FIN LECTURE #

VEFPreP1B dis
schema_Adams_Bashforth_order_3 sch
Lire sch
{
	tmax 0.015
	seuil_statio -1
	seuil_statio_relatif 1
	dt_impr -1
	facsec 0.5 # facsec 1.0 diverges #
}

Fluide_Incompressible air
Lire air
{
	mu  Champ_Uniforme 1 0.01
	rho Champ_Uniforme 1 1
}

Associate pb dom
Associate pb sch
Associate pb air
Discretize pb dis

Lire pb
{
	Navier_Stokes_Standard
	{
		
		solveur_pression petsc cholesky { }
		convection { muscl }
		diffusion {  }
		conditions_initiales 
		{
			vitesse 	Champ_Fonc_txyz dom 2 -cos(2*Pi*x)*sin(2*Pi*y)*exp(-8*Pi*Pi*0.01*t) sin(2*Pi*x)*cos(2*Pi*y)*exp(-8*Pi*Pi*0.01*t)
		}
		boundary_conditions 
		{
			BOUNDARY symetrie
		}
	}
        Postraitement
	{
		Format lml
                Champs dt_post 2
		{
		    pression 				som
		    vitesse 				elem
		}
       }
}

Solve pb
Fin
