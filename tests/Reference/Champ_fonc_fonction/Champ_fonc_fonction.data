# Test de Champ_fonc_fonction #
# PARALLEL OK 8 #
dimension 2

Pb_Thermohydraulique_Concentration pb
Domaine dom

# BEGIN MESH #
Mailler dom
{
	Pave Cavite 
		{
		Origine 0. 0.
		Nombre_de_Noeuds 11 11 
		Longueurs 0.01 0.01
		}
		{
		Bord Gauche X = 0.	0. <= Y <= 0.01
		Bord Haut   Y = 0.01	0. <= X <= 0.01
		Bord Bas    Y = 0.	0. <= X <= 0.01
		Bord Droit  X = 0.01	0. <= Y <= 0.01
		}
}
trianguler_H dom
# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 2
	Nom_Zones DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1B dis

Scheme_euler_explicit sch
Read sch
{
	nb_pas_dt_max 1
	tinit 0
	tmax 1.
	dt_min 1.e-8
	dt_max 0.01
	dt_impr 0.000001
	dt_sauv 100
	seuil_statio 1.e-8
}

Fluide_Incompressible fluide
Read fluide
{
	mu	Champ_Fonc_Fonction 1 temperature 1.85e-5*(1+val*0.01)
	rho	Champ_Uniforme 1 1.
	lambda	Champ_Fonc_Fonction 1 temperature 0.0262
	Cp	Champ_Uniforme 1 1.
	beta_th	Champ_Uniforme 1 3.41e-3
	beta_co	Champ_Uniforme 1 0.04
}

Constituant c
Read c
{
	coefficient_diffusion Champ_Fonc_Fonction 1 concentration 0.0262
}

Champ_Uniforme gravite
Read gravite 2 0 -9.81

Associate fluide gravite
Associate pb dom
Associate pb sch
Associate pb fluide
Associate pb c

Discretize pb dis

Read pb
{
	Navier_Stokes_standard
	{
	 	solveur_pression GCP { 
			precond ssor { omega 1.5 } 
			seuil 1.e-10
			impr 
		}
		convection { amont }
		diffusion { }
		sources { boussinesq_temperature { T0 5. } , boussinesq_concentration { C0 1 1. } }
		conditions_initiales
		{
			vitesse Champ_Uniforme 2 0. 0.
		}
		boundary_conditions
		{
			Haut	frontiere_ouverte_vitesse_imposee Champ_Front_Uniforme 2  0. 0.
			Droit	frontiere_ouverte_vitesse_imposee Champ_Front_Uniforme 2  0. 0.
			Bas	frontiere_ouverte_vitesse_imposee Champ_Front_Uniforme 2  0. 0.
			Gauche	frontiere_ouverte_vitesse_imposee Champ_Front_Uniforme 2  0. 0.
		}
		
	}
	Convection_Diffusion_Temperature
	{
		diffusion { }
		convection { amont }
		conditions_initiales { Temperature Champ_Uniforme 1 5. }		
		boundary_conditions 
		{
			Haut	frontiere_ouverte_temperature_imposee Champ_Front_Uniforme 1 0.
			Bas	frontiere_ouverte_temperature_imposee Champ_Front_Uniforme 1 0.
 			Gauche	frontiere_ouverte_temperature_imposee Champ_Front_Uniforme 1 0.
			Droit	frontiere_ouverte_temperature_imposee Champ_Front_Uniforme 1 10.
		}
		sources { puissance_thermique champ_fonc_fonction 1 concentration val }
	}
	Convection_diffusion_Concentration
	{
		diffusion { }
		convection { amont }		
		conditions_initiales { concentration Champ_Uniforme 1 5. }
		boundary_conditions
		{
			Haut	frontiere_ouverte_concentration_imposee Champ_Front_Uniforme 1 0.
			Bas	frontiere_ouverte_concentration_imposee Champ_Front_Uniforme 1 0.
			Gauche	frontiere_ouverte_concentration_imposee Champ_Front_Uniforme 1 0.
			Droit	frontiere_ouverte_concentration_imposee Champ_Front_Uniforme 1 10.
		}
		sources { source_constituant champ_fonc_fonction 1 temperature val }
	}
	Postraitement
	{
		Sondes
		{
			temperature 	temperature 	periode 1.e-8 points 1 0.005 0.005
			concentration	concentration	periode 1.e-8 points 1 0.005 0.005
		
		}
		format lata
		Champs dt_post 0.1
		{
			temperature		elem
			concentration 		elem
			diffusivite_thermique	elem
			coefficient_diffusion 	elem
		}
	}
}

Solve pb
End

