# PARALLEL OK #
dimension 3
Domaine dom

# BEGIN MESH #
Mailler dom
{
Pave Cavite
        {
                Origine 0.  0. 0.
                Nombre_de_Noeuds 5 5 5
                Longueurs 1 1 1
        }
        {
                Bord perio   	X = 0.   	0. <= Y <= 1.    	0. <= Z <= 1.
                Bord perio  	X = 1.  	0. <= Y <= 1.   	0. <= Z <= 1.
                Bord paroi   	Y = 0.  	0. <= X <= 1.  		0. <= Z <= 1.
                Bord paroi   	Y = 1. 	 	0. <= X <= 1.  		0. <= Z <= 1.
                Bord paroi   	Z = 0.  	0. <= X <= 1.  		0. <= Y <= 1.
                Bord paroi   	Z = 1.  	0. <= X <= 1.   	0. <= Y <= 1.
        }
} 
Tetraedriser_homogene dom
Transformer dom
x+4*x*(1-x)*y*(1-y)*z*(1-z)*(0.5-y)
y+4*x*(1-x)*y*(1-y)*z*(1-z)*(0.5+x)
z
Corriger_frontiere_periodique { domaine dom bord perio }
# END MESH #

# BEGIN PARTITION
Partition dom
{
	Partitionneur Tranche { tranches 1 2 1 }
	periodique 1 perio
	Larg_joint 2
	Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1b dis

Schema_Euler_Explicite sch
Lire sch 
{
	nb_pas_dt_max 10
        tinit 0.
        tmax 0.1
        dt_min 1.e-6
        dt_max 1.e-1
        dt_impr 1.e-6
        dt_sauv 20.
        seuil_statio 1.e-8  
}
Fluide_Incompressible fluide
Lire fluide
{
	mu Champ_Uniforme	1 2.
	rho Champ_Uniforme	1 3.
}

Pb_Hydraulique pb
Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis
Lire pb
	{
	Navier_Stokes_standard
		{
		dt_projection 1. 1.e-16

		Solveur_Pression GCP { 
			precond ssor { omega 1.6 }
			seuil 1.e-11 impr 
		} 

		solveur_bar Cholesky { }

                convection { negligeable  }
                diffusion  {  }
				
 
		conditions_initiales
			{
                        vitesse Champ_fonc_xyz dom 3  2. 0. 0.
			}
		boundary_conditions
			{
			paroi 	paroi_fixe
			perio 	periodique 
			}
		 Traitement_particulier { chmoy_faceperio { stats 0 0.2 } }
		}

	Postraitements
	{
		lml
        	{
		fichier prepare
		format lml  
                Sondes
                	{ 
                        sonde_vit1  vitesse periode 0.01 points 1 0.5 0.5 0.5 
			sonde_pression pression periode 0.01 segment 10 0. 0.5 0.5      1. 0.5 0.5
			}
		Champs dt_post 2.
			{
			pression elem
			vitesse elem
			}
		}
	}
}

Solve pb
Fin
