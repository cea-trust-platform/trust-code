# Hydraulique 2D VEF K-Eps Bi-cylindres sans loi de paroi #
# Cylindre ext : vitesse imposee #
# Cylindre interieur : symetrie #
dimension 2
Pb_hydraulique_turbulent pb
Domaine dom
Read_file dom RT.geom
VEF dis

Scheme_euler_explicit sch
Read sch
{
	tinit 0.
	tmax 20.
	dt_min 0.00001
	dt_max 0.1
	dt_impr 0.0001
	dt_sauv 100
	seuil_statio 1.e-8
	facsec 0.5
}

Fluide_Incompressible fluide
Read fluide
{
	mu Champ_Uniforme 1 1e-4
	rho Champ_Uniforme 1 1.
}
Sous_Zone zone1
Associate zone1 dom
Read zone1
{
   Rectangle
   Origine -1. -0.1
   Cotes 0.2 0.2
}

Champ_Uniforme gravite
Read gravite 2 0 -9.81

Associate fluide gravite
Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis

Read pb
{
	Navier_Stokes_turbulent
	{
		solveur_pression GCP { precond ssor { omega 1.65 } seuil 1.e-8 impr }	
		convection { muscl }
		diffusion { }
		conditions_initiales {
			vitesse tourbillon
			}
		boundary_conditions {
			CERCLE frontiere_ouverte_vitesse_imposee 
			champ_front_calc pb CERCLE vitesse
			INTER frontiere_ouverte_vitesse_imposee
			champ_front_calc pb INTER vitesse
		}
                modele_turbulence K_Epsilon {
                        Transport_K_Epsilon 
                        {
                                convection { amont }
                                diffusion { negligeable }
                                boundary_conditions {
                                        CERCLE symetrie
                                        INTER symetrie
                                }
                                conditions_initiales {
                                        k_Eps Champ_Uniforme_Morceaux dom 2
					    { defaut 0. 0 zone1 0.1 0.01 }
                                }
                         }
                         turbulence_paroi negligeable
                } 
	}

	Postraitement
	{
		Sondes
		{
			plan_k k periode 0.0001 plan 20 20 -1.5 -1.5 1.5 -1.5 -1.5 1.5
			plan_eps eps periode 0.0001 plan 20 20 -1.5 -1.5 1.5 -1.5 -1.5 1.5
			plan_nut viscosite_turbulente periode 0.0001 plan 20 20 -1.5 -1.5 1.5 -1.5 -1.5 1.5
		}
		Champs dt_post 0.1
		{
			pression elem
			pression som
			vitesse som
			viscosite_turbulente elem
			viscosite_turbulente som
			k som
			eps som
		}
	}
}

Solve pb
End
