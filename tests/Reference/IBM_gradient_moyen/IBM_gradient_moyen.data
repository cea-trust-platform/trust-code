# Hydraulique 2D laminaire POISEUILLE #
/*  Re = (rho*U_max*L)/mu
  rho = 1.0 kg/m^3
  U_max = 1 m/s
  L = 1 m
  Re = 1
  mu = (rho*U_max*L)/Re
  A.N.
  mu = 1.0 */
# PARALLEL OK #
dimension 3
Pb_Hydraulique_ibm pb
Domaine dom
system "mkdir -p results"

# DEBUT MAILLAGE #
Read_MED {
    domain dom
    file res_domain_mesh_40_cyl_mesh_250fact12.med
}

regroupebord dom Wall { TOP }
regroupebord dom Postpro { BOTTOM }
regroupebord dom Sortie { IN OUT DOWN UP }
# FIN MAILLAGE #

# DEBUT DECOUPAGE
Partition dom
{
    Partition_tool tranche { tranches 2 2 1 }
    Larg_joint 2
    zones_name DOM
}
End
FIN DECOUPAGE #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

# DISCRETISATION EF ---------------------------------- #

EF dis

# SCHEMA EN TEMPS ------------------------------------ #

Schema_Euler_implicite sch
Lire sch
{
    tinit 0
    tmax 55.
    dt_min 5.e-8
    dt_impr 5.e-1
    dt_sauv 100.
    seuil_statio 1.e-4
    solveur implicite
    {
        seuil_convergence_implicite 1.e-10
        seuil_convergence_solveur  1e-10
        solveur Petsc IBICGSTAB { precond diag { } seuil 1e-10 }
    }
    dt_start dt_calc
    dt_max 0.1

    nb_pas_dt_max 1
    facsec 2.5
}

Associer pb dom
Associer pb sch
Discretiser pb dis
# Debog pb seq faces 1e-6 0 #
# postraiter_domaine { domaine dom format lata fichier geom } #

Domaine out_chaud
extraire_surface
{
    domaine out_chaud
    probleme pb
    avec_certains_bords 1 Sortie
}
Domaine wall
extraire_surface
{
    domaine wall
    probleme pb
    avec_certains_bords 1 Wall
}
Lire pb
{

    fluide_incompressible {
        mu  Champ_Uniforme 1 0.25
        rho Champ_Uniforme 1 1.0
    }


    Navier_Stokes_ibm
    {
        correction_matrice_projection_initiale 0
        correction_calcul_pression_initiale 0
        correction_vitesse_projection_initiale 1
        projection_initiale 0
        methode_calcul_pression_initiale avec_sources_et_operateurs
        correction_matrice_pression 1
        correction_vitesse_modifie 1
        gradient_pression_qdm_modifie 0
        correction_pression_modifie 0
        convection { generic centre }
        solveur_pression petsc cholesky { }
        diffusion { option { grad_u_transpose_partout 1 } }
        sources
        {
            # Attention: cas test qui verifie seulement que le code lit les mots cles prevus de cette interpolation #
            Source_PDF
            {
                transpose_rotation
                aire Champ_Fonc_MED {
                    domain domain_mesh
                    file res_domain_mesh_40_cyl_mesh_250fact12.med
                    field aire
                    last_time
                }
                rotation Champ_Fonc_MED {
                    domain domain_mesh
                    file res_domain_mesh_40_cyl_mesh_250fact12.med
                    field rotation
                    last_time
                }
                modele
                {
                    echelle_relaxation_coefficient_pdf 1
                    temps_relaxation_coefficient_pdf 1
                    eta 1.E-10
                    local
                    vitesse_imposee_fonction 3 0. (-(((x*x)+(y*y))<=(0.75*0.75))*0.5+(((x*x)+(y*y))>=(0.75*0.75))*1.0) 0.
                }
                interpolation IBM_gradient_moyen
                {
                    est_dirichlet Champ_Uniforme 1 0
                    points_solides Champ_Fonc_MED {
                        domain domain_mesh
                        file res_domain_mesh_40_cyl_mesh_250fact12.med
                        field projection_Solid
                        loc som
                        last_time
                    }
                    elements_solides Champ_Fonc_MED {
                        domain domain_mesh
                        file res_domain_mesh_40_cyl_mesh_250fact12.med
                        field fl_elem_number
                        loc som
                        last_time
                    }
                    correspondance_elements Champ_Fonc_MED {
                        domain domain_mesh
                        file res_domain_mesh_40_cyl_mesh_250fact12.med
                        field TRUST_SALOME_correspondance
                        last_time
                    }
                }
            }
        }
        conditions_initiales
        {
            vitesse champ_uniforme 3 0. 0. 0.
        }
        conditions_limites
        {
            Wall Symetrie
            Postpro Symetrie
            Sortie frontiere_ouverte_pression_imposee Champ_front_Uniforme 1 1
        }
    }
    Post_processing
    {
        format lml
        Champs dt_post 1e0
        {
            pression elem
            vitesse som
        }
    }
}
resoudre pb
fin
