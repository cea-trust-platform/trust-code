# Hydraulique 2D laminaire avec "trou" dans le maillage #
# PARALLEL OK #
dimension 2
Pb_hydraulique pb
Domaine dom

# BEGIN MESH #
Mailler dom 
{
	Pave pave1
		{
		Origine 0. 0.
		Longueurs 10 2
		Nombre_de_Noeuds 6 11
                Facteurs 1. 1.
		}
		{
		bord Inlet 	X = 0. 0. <= Y <= 2.
		bord Upper 	Y = 2. 0. <= X <= 10.
		bord Lower 	Y = 0. 0. <= X <= 10.
		bord Outlet	X = 10. 0. <= Y <= 2.
		}
}
# END MESH #
# BEGIN PARTITION

Partition dom
{
	Partition_tool tranche { tranches 2 1 }  
	Larg_joint 2
	zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VDF dis

Scheme_euler_explicit sch
Read sch
{
        tinit 0
	nb_pas_dt_max 1
        dt_min 5.e-8
        dt_impr 5.e-1
        seuil_statio 1.e-6
	dt_max 1
}

Fluide_Incompressible eau
Read eau
{
	mu  Champ_uniforme 1 1.
	rho Champ_Uniforme 1 1000.
}

Associate pb dom
Associate pb sch
Associate pb eau

Discretize pb dis

Read pb
{
	Navier_Stokes_standard
	{
		solveur_pression GCP { 
			precond ssor { omega 1.5 } 
			seuil 1.e-8
			impr
		}
		diffusion {  }
		convection { amont }
		initial_conditions { vitesse Champ_fonc_xyz dom  2 0 0 }
		boundary_conditions {
			Upper  paroi_fixe
			Lower  paroi_fixe
			Inlet  frontiere_ouverte_pression_imposee champ_front_uniforme 1 0
			Outlet  frontiere_ouverte_vitesse_imposee champ_front_xyz_debit { 
                                flow_rate champ_front_uniforme 1 1
				velocity_profil champ_front_fonc_xyz 2 (y_LT_0.5)*y+((y_GE_0.5)_AND_(y_LE_1.5))*0.5+(y_GT_1.5)*(2-y) 0 
                        }
		}
	}

	Post_processing
	{
		definition_champs
		{
			debit_elem Interpolation
			{
				domaine dom_boundaries_Outlet
				localisation elem
				source Morceau_Equation {
				type operateur numero 3 option flux_bords
				source refChamp { Pb_champ pb vitesse }
				}
			}
			debit Reduction_0D { methode sum sources_reference { debit_elem } }
		}
		Probes
		{
			debit_Outlet debit         periode 0.01 numero_elem_sur_maitre 0
			profil_vitX   nodes vitesseX periode 0.01 segment 10 10 0 10 2
			profil_vitY   nodes vitesseY periode 0.01 segment 10 10 0 10 2
		}
		format lml
		fields dt_post 1.3
		{
			vitesse som
		}
	}
}
Solve pb
End

