# Hydraulique 2D : Poiseuille 2D #
# PARALLEL OK #
# uc=DP/L/8.=0.125 m/s #
dimension 2
Pb_hydraulique pb
Domaine dom
# BEGIN MESH #
Mailler dom
{
        Pave Entree
                {
                Origine 0. 0.
                Nombre_de_Noeuds  3 6
                Longueurs 1 1
                }
                {
                bord Entree X = 0.   0. <= Y <= 1.0
                bord Haut   Y = 1.0  0. <= X <= 1.0
                bord Bas    Y = 0.   0. <= X <= 1.0
                bord Sortie X = 1.0  0. <= Y <= 1.0
                }
}
Trianguler_h dom
# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 2
	Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #
VEFPrep1b dis

Schema_Euler_implicite sch
Lire sch
{
nb_pas_dt_max 200
	tinit 0
	tmax 5000.
	dt_min 0.0001e-5
	dt_start dt_calc
	dt_max 1.0
	dt_sauv 100
	seuil_statio 1.e-6
max_iter_implicite 10
solveur simpler
{
                seuil_convergence_implicite 1.e-4
                seuil_convergence_solveur 1.e-8
solveur gmres { diag seuil 1.e-8 }
}
facsec 10
}


Fluide_Incompressible eau
Lire eau
{
	mu Champ_Uniforme 1 1.
	rho Champ_Uniforme 1 1.
}

Champ_Uniforme gravite
Lire gravite 2 0 0
Associate pb dom
Associate pb sch
Associate pb eau
Discretize pb dis
# Debog pb seq faces 1.e-6 0 #
Lire pb
{
	Navier_Stokes_standard
	{
		solveur_pression GCP  { precond_nul seuil 1e-6 impr }

		convection { negligeable }
		diffusion { }
		conditions_initiales {
			vitesse Champ_Uniforme 2 0.1 0.
			}
		boundary_conditions {
			Haut paroi_fixe
			Bas paroi_fixe
			Entree frontiere_ouverte_pression_imposee Champ_Front_Uniforme 1 1.
			Sortie frontiere_ouverte_pression_imposee Champ_Front_Uniforme 1 0.
		}
	}
	Postraitement
	{
		Sondes
		{
		 seg_P pression periode 0.1 segment 20 0.025 0.5 0.975 0.5
		 seg_U1 vitesse periode 0.1 segment 11 0. 0.5 1.0 0.5
		 seg_U2 vitesse periode 0.1 segment 19 1.0 0.02631 1.0 0.9736
		 sonde_pression pression periode 0.1 points 1 0.5 0.5
		 sonde_vitesse  vitesse periode 0.1 points 1 0.5 0.5
		}
		Champs dt_post .1
		{
			pression elem
			pression som
			vitesse elem
			vitesse som
		}
	}
}
Solve pb
Fin
Fin
