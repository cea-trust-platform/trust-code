# Thermohydraulique 2D : reprise calcul #
# PARALLEL OK #
dimension 2
Pb_Thermohydraulique pb
Domaine dom
# DEBUT MAILLAGE #
Mailler dom
{
	Pave Cavite 
		{
		Origine 0. 0.
     		Nombre_de_Noeuds 11 11
		Longueurs 0.01 0.01
		}
		{
		Bord Gauche X = 0. 0. <= Y <= 0.01
		Bord Haut Y = 0.01 0. <= X <= 0.01
		Bord Bas Y = 0. 0. <= X <= 0.01
		Bord Droit X = 0.01 0. <= Y <= 0.01
		}
}

# FIN MAILLAGE #
# DEBUT DECOUPAGE
Decouper dom
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 2
	Nom_Zones DOM
}
Fin
FIN DECOUPAGE #

# DEBUT LECTURE
Scatter DOM.Zones dom
FIN LECTURE #

VDF dis
Schema_Euler_explicite sch
Lire sch
{
	tinit 0.2
	tmax 1.2
	dt_min 0.0001
	dt_max 0.01
	dt_impr 0.0001
	dt_sauv 0.01
	seuil_statio 1.e-8
}

Fluide_Incompressible fluide
Lire fluide
{
	mu Champ_Uniforme 1 1.85e-5
	rho Champ_Uniforme 1 1.17
	lambda Champ_Uniforme 1 0.0262
	Cp Champ_Uniforme 1 1006
	beta_th Champ_Uniforme 1 3.41e-3
}
Champ_Uniforme gravite
Lire gravite 2 0 -9.81

Associer fluide gravite
Associer pb dom
Associer pb sch
Associer pb fluide

Discretiser pb dis

Lire pb
{
	Navier_Stokes_standard
	{
		solveur_pression GCP { 
			precond ssor { omega 1.5 } 
			seuil 1.e-12
			impr 
		}
		convection { Quick }
		diffusion { }
		sources { boussinesq_temperature { T0 5. } }
		conditions_initiales {
			vitesse Champ_Uniforme 2 0. 0.
			}
		conditions_limites {
			Haut paroi_fixe
			Bas paroi_fixe
			Gauche paroi_fixe
			Droit paroi_fixe
		}
	}
	Convection_Diffusion_Temperature
	{
		diffusion { }
		convection { quick }
		conditions_limites 
		{
			Haut paroi_adiabatique
			Bas paroi_adiabatique
 			Gauche paroi_temperature_imposee Champ_Front_Uniforme 1 10.
			Droit  paroi_temperature_imposee Champ_Front_Uniforme 1 0.  
		}
		conditions_initiales { Temperature Champ_Uniforme 1 5. }
	}

	Postraitements
	{
		lml {
		Sondes
		{
			sonde_pression pression periode 0.01 points 2 0.0005 0.0045 0.0045 0.0045
			sonde_vitesse vitesse periode 0.01 points 5 0.0005 0.004 0.0015 0.004 0.0025 0.004 0.0035 0.004 0.0045 0.004
			sonde_temp temperature periode 0.01 points 2 0.0005 0.0045 0.0045 0.0045
		}
		format lml
		fichier resume_last_time_xyz
		Champs dt_post 0.1
		{
			pression elem
			pression som
			vitesse elem
			vitesse som
			temperature elem
			temperature som
		}
		}
		lata
		{
		format lata
		fichier Resume_last_time
		Champs dt_post 0.1
		{
			pression elem
			pression som
			vitesse elem
			vitesse som
			temperature elem
			temperature som
		}		
		}
	}
	resume_last_time xyz sauve.rep
}

Resoudre pb
Fin
Fin
