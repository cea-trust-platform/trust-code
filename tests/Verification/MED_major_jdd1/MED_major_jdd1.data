# Hydraulique 2D laminaire avec "trou" dans le maillage #
# PARALLEL NOT #
dimension 2
Pb_hydraulique pb
Domaine dom
# BEGIN MESH #
Read_file upwind.geo ;

# END MESH #

VDF dis

Scheme_euler_explicit sch
Read sch
{
	tinit 0
	nb_pas_dt_max 1
	tmax 2.5
	/* dt_min identique au dt_max alors dt=dt_min=dt_max */
	dt_min 5.e-3
	dt_max 5.e-3
	dt_impr 5.e-1
	dt_sauv 1
	seuil_statio 1.e-8
}

Fluide_Incompressible eau
Read eau
{
	mu Champ_Uniforme 1 3.7e-05
	rho Champ_Uniforme 1 2
}

Associate pb dom
Associate pb sch
Associate pb eau

Discretize pb dis

Read pb
{
	Navier_Stokes_standard
	{
		solveur_pression GCP { 
			precond ssor { omega 1.5 } 
			seuil 1.e-8 impr
		}
		convection { amont }
		diffusion {  }
		# Conditions initiales sur la vitesse #
		initial_conditions {
			vitesse Champ_Uniforme 2 0. 0.
			}
		boundary_conditions {
			Obstacle paroi_fixe
			Symetrie symetrie 
			Sortie frontiere_ouverte_pression_imposee  Champ_front_Uniforme 1 0.
			Entree frontiere_ouverte_vitesse_imposee    Champ_front_Uniforme 2  1. 0.
		}
	}

	Post_processing
	{
		Format med_major
		fields dt_post 1.0
		{
			vitesse elem
		}
	}

}
Solve pb
End

