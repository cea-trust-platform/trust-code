# Hydraulique 2D turbulent K-Eps #
# PARALLEL RUNS #
dimension 2
Pb_Hydraulique pb
Domaine dom
# BEGIN MESH #
Read_file_binary dom in_out.geom
precisiongeom 1e-5
# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 1
	Nom_Zones DOM
}
End

END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

Vefprep1b dis
schema_Euler_explicite sch
Read sch
{
	tinit 0
	nb_pas_dt_max 3
tmax 5	
	dt_min 0.1
	 dt_max 0.1 
	
	dt_impr 0.1
	dt_sauv 1000.
	seuil_statio 1.e-18
/* solveur implicite { solveur gmres { diag seuil 1e-20 nb_it_max 3 } } */
}
Fluide_Incompressible fluide
Read fluide
{
	mu Champ_Uniforme 1 8e-4
	rho Champ_Uniforme 1 2
}
Associate pb dom
Associate pb sch
Associate pb fluide

VerifierCoin dom { }

Read dis {  P0 P1 changement_de_base_P1bulle 1 CL_pression_sommet_faible 1   modif_div_face_dirichlet 1   }
ecrire dis

Discretize pb dis

Read pb
{
	Navier_Stokes_standard
	{
		solveur_pression gcp { precond ssor { omega 1.6 } seuil 1e-6 }
		solveur_pression cholesky { }
		convection { negligeable }
		diffusion { negligeable  }
		conditions_initiales {
			vitesse champ_uniforme 2 -1. 0
			}
		sources { source_qdm champ_fonc_xyz dom 2 2 0 }
			
		boundary_conditions {
			 Haut Paroi_Fixe
			 Bas Paroi_Fixe
		gauche frontiere_ouverte_pression_imposee champ_front_uniforme 1 0 
	
		droite frontiere_ouverte_vitesse_imposee champ_front_uniforme 2 -1 0 
	 
		}
	}
Postraitement 
{
		Definition_champs {
vmax champ_post_reduction_0d { methode max source champ_post_refchamp { pb_champ pb vitesse } }
}
		Sondes
		{
		vmax vmax periode 1e-5 numero_elem_sur_maitre 0	
		gra gradient_pression periode 0.1 segment 20 0 1 15 1
		sonde_vitesse vitesse periode 0.01 points 1 10. 0.5
	}
	format lml
	Champs dt_post 20.
	{
			pression elem
			gradient_pression
			pression som
	}
}
/* on met la vitesse et div dans un autr e post pour eviter les ecarts */
Postraitements { 
VIT 
{
   format lml
        Champs dt_post 20.
        {
	 divergence_u elem
	vitesse

}
}
}
}
Solve pb
End
