# PARALLEL OK 2 #
dimension 2
Pb_Hydraulique pb
Domaine dom
# BEGIN MESH #
Mailler dom
{	Pave Cavite
	{
		Origine 0. 0. 
		Nombre_de_Noeuds 11 11
		Longueurs 2. 2. 
	}
	{
		Bord entree  X = 0.            	0. <= Y <= 2.0          
		Bord sortie  X = 2.0       	0. <= Y <= 2.0           
		Bord bas     Y = 0.            	0. <= X <= 2.0           
		Bord haut    Y = 2.0           	0. <= X <= 2.0           
		} 	
}

Trianguler_H dom

# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partition_tool tranche { tranches 2 1 }
	Larg_joint 2
	zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

 VEFPreP1B dis

Scheme_euler_explicit sch
Read sch
{
	tinit 0
	nb_pas_dt_max 0
	tmax 1e-5
	dt_impr 0
	dt_sauv 0
	seuil_statio 1e5
}
Fluide_Incompressible fluide
Read fluide
{
	mu Champ_Uniforme 1 3.7e-05
	rho Champ_Uniforme 1 1000
}
Associate pb dom
Associate pb sch
Associate pb fluide
Discretize pb dis
Read pb
{

	 Navier_Stokes_standard
	 {
	    	projection_initiale 0
	      	equation_non_resolue 1
	      	solveur_pression petsc cholesky { }
	      	convection { negligeable }
	      	diffusion { negligeable }
	      	initial_conditions
		{
	      		vitesse Champ_fonc_xyz dom 2 x+2*y 1/2*x*x+1/2*y*y
	      	}
	      	boundary_conditions {
	      		entree frontiere_ouverte_vitesse_imposee  Champ_Front_Fonc_xyz 2 x+2*y 1/2*x*x+1/2*y*y 
	      	        sortie frontiere_ouverte_vitesse_imposee  Champ_Front_Fonc_xyz 2 x+2*y 1/2*x*x+1/2*y*y 
	      		bas  frontiere_ouverte_vitesse_imposee  Champ_Front_Fonc_xyz 2 x+2*y 1/2*x*x+1/2*y*y 
	                haut   frontiere_ouverte_vitesse_imposee Champ_Front_Fonc_xyz 2 x+2*y 1/2*x*x+1/2*y*y 
	      	}
	  }




        Post_processing
	{

                definition_champs
                {
                        DU_DX Transformation { methode composante numero 0 source refChamp { Pb_champ pb grad_u } localisation elem }
                        DU_DY Transformation { methode composante numero 1 source refChamp { Pb_champ pb grad_u } localisation elem }
                        DV_DX Transformation { methode composante numero 2 source refChamp { Pb_champ pb grad_u } localisation elem }
                        DV_DY Transformation { methode composante numero 3 source refChamp { Pb_champ pb grad_u } localisation elem }
                }


	 	format lml
		fields dt_post 0.
		{
			vitesse
			DU_DX elem
			DU_DY elem
			DV_DX elem
			DV_DY elem
		}


	}
}

Solve pb
End
