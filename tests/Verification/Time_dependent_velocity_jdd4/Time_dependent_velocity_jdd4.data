# PARALLEL OK 2 #
dimension 2
Pb_Hydraulique pb
Domaine dom
# BEGIN MESH #
Read_file mesh.geo ;

trianguler_h dom
# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partition_tool tranche { tranches 2 1 }
	Larg_joint 2
	zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFprep1b dis
Scheme_euler_explicit sch
Read sch
{
	dt_max 5e-2
	dt_min 5e-2
	tinit 0
	nb_pas_dt_max 3
	tmax 5.
	dt_impr 0.1
	dt_sauv 1000.
}
Fluide_Incompressible fluide
Read fluide
{
	mu Champ_Uniforme 1 3.7e-05
	rho Champ_Uniforme 1 1000
}
Associate pb dom
Associate pb sch
Associate pb fluide
Discretize pb dis

domaine surface
extraire_surface
{
	domaine surface
	probleme pb
	condition_elements (x<2)
	condition_faces (y<1e-6)
	avec_les_bords
}
# postraiter_domaine { domaine surface format lml fichier surface } #

Read pb
{
	Navier_Stokes_standard
	{
		solveur_pression petsc cholesky { }
		convection { negligeable }
		diffusion { }
		initial_conditions {
			vitesse Champ_Uniforme 2 0. 0.
			}
		boundary_conditions {
			inlet      frontiere_ouverte_vitesse_imposee  champ_front_fonc_t   2 0. 2.*t*t+0.1
			outlet     frontiere_ouverte_pression_imposee champ_front_uniforme 1 0
			bottomwall symetrie
			topwall    symetrie
		}
	}
	Post_processing
	{
		Definition_champs {
				inlet_velocity Interpolation {
					localisation elem
					domaine surface
					source refChamp {
						pb_champ  pb vitesse
					}
				}
				mean_inlet_velocity reduction_0D {
					methode average
					sources_reference { inlet_velocity }
				}
			}
		probes {
			Vitesse_u mean_inlet_velocity periode 5e-2 numero_elem_sur_maitre 0
		}
		format lml
		fields dt_post 1.
		{
			vitesse elem
		}
	}
}

Solve pb
End
