file(GLOB srcs [A-Z|a-z]*.cpp [A-Z|a-z]*.c [A-Z|a-z]*.f)
include_directories(${CMAKE_CURRENT_SOURCE_DIR}  ${TRUST_ROOT}/include/backward )
add_library(obj_ijk_vecto OBJECT  ${srcs} )
set(listlibs ${listlibs} ijk_vecto PARENT_SCOPE    )
add_custom_target(check_sources_ijk_vecto   COMMAND check_sources.sh ${CMAKE_CURRENT_SOURCE_DIR} ) #COMMENT  "checking code validity ijk_vecto" )
add_DEPENDENCIES(obj_ijk_vecto check_sources_ijk_vecto)
if(COMPIL_DYN)
  add_library(ijk_vecto SHARED   $<TARGET_OBJECTS:obj_ijk_vecto> )
  install(TARGETS ijk_vecto  DESTINATION lib)
endif(COMPIL_DYN)
