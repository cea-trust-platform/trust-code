# SIMULATION D'UNE BOITE 2D VEF EN THERMOHYDRAULIQUE COMPRESSIBLE #

dimension 2



Domaine dom1
Domaine dom2
Domaine dom3
Domaine dom4

# BEGIN MESH #
Mailler dom1
{
	Pave Cavite
		{
		Origine 0. 0.
		Nombre_de_Noeuds 2 2
		Longueurs  1. 1.
		}
		{
		Bord Gauche	X = 0.	0. <= Y <= 1.
		Bord Droit	X = 1.	0. <= Y <= 1.
		Bord Bas	Y = 0.	0. <= X <= 1.
		Bord Haut	Y = 1.	0. <= X <= 1.
		}
}
Mailler dom2
{
	Pave Cavite
		{
		Origine 0. 0.
		Nombre_de_Noeuds 2 2
		Longueurs  1. 1.
		}
		{
		Bord Gauche	X = 0.	0. <= Y <= 1.
		Bord Droit	X = 1.	0. <= Y <= 1.
		Bord Bas	Y = 0.	0. <= X <= 1.
		Bord Haut	Y = 1.	0. <= X <= 1.
		}
}
Mailler dom3
{
	Pave Cavite
		{
		Origine 0. 0.
		Nombre_de_Noeuds 2 2
		Longueurs  1. 1.
		}
		{
