		{
		Origine 0. 0.
		Nombre_de_Noeuds 2 2
		Longueurs  1. 1.
		}
		{
		Bord Gauche	X = 0.	0. <= Y <= 1.
		Bord Droit	X = 1.	0. <= Y <= 1.
		Bord Bas	Y = 0.	0. <= X <= 1.
		Bord Haut	Y = 1.	0. <= X <= 1.
		}
}
# END MESH #

VDF dis


Pb_conduction pb_1
Scheme_euler_explicit sch_1
Read sch_1
{
	tinit	0.
	tmax	15.
	dt_min	1.e-10
	dt_max	1.e-1
	dt_impr	1.
	dt_sauv	15.
        seuil_statio 1.e-12
}

Solide solide_1
Read solide_1
{
	rho champ_uniforme 1  1
	cp  champ_uniforme 1 1
	lambda champ_uniforme 1 0.01 
}


Associate pb_1 dom1
Associate pb_1 solide_1



 

#  #
Pb_conduction pb_2
runge_kutta_rationnel_ordre_2 sch_2
Read sch_2
{
	tinit	0.
	tmax	15.
	dt_min	1.e-10
	dt_max	1.e-1
	dt_impr	1.
	dt_sauv	15.
        seuil_statio 1.e-12
}

Solide solide_2
Read solide_2
{
	rho champ_uniforme 1  1
	cp  champ_uniforme 1 1
	lambda champ_uniforme 1 0.01 
}


Associate pb_2 dom2
Associate pb_2 solide_2


Pb_conduction pb_3
runge_kutta_ordre_3 sch_3
Read sch_3
{
	tinit	0.
	tmax	15.
	dt_min	1.e-10
	dt_max	1.e-1
	dt_impr	1.
	dt_sauv	15.
        seuil_statio 1.e-12
