	Conduction
	{
		diffusion { }	
		
		boundary_conditions 
		{
			Haut		paroi_echange_global_impose h_imp champ_front_uniforme 1 1 t_ext  Champ_Front_Uniforme 1 50.
			Bas		Symetrie
