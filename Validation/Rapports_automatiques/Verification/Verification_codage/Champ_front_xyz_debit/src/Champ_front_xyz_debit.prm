Parameters {
	Title "Champ_front_xyz_debit verification"
	Author "triou"
	VersionTrio_U 1.7.8_beta
	TestCase Champ_front_xyz_debit_0    Champ_front_xyz_debit_0.data
	TestCase Champ_front_xyz_debit_1    Champ_front_xyz_debit_1.data
	TestCase Champ_front_xyz_debit_2    Champ_front_xyz_debit_2.data
	TestCase Champ_front_xyz_debit_3    Champ_front_xyz_debit_3.data
	TestCase Champ_front_xyz_debit_4    Champ_front_xyz_debit_4.data
}


Chapter {
	Title "Visualization for 2D VEF test case, uniforme fields" 
	Table {
        Title "Boundary conditions"
        Description include_text_file(Champ_front_xyz_debit_0/BC.txt, boundary_conditions, vitesse, Champ_Uniforme, paroi_fixe, Frontiere_ouverte_vitesse_imposee, Champ_front_Uniforme, Frontiere_ouverte_pression_imposee, Champ_front_tabule, champ_front_xyz_debit, champ_front_fonc_xyz, flow_rate, velocity_profil )
    }
	visu {
		title "Mesh"
		mesh  Champ_front_xyz_debit_0/Champ_front_xyz_debit_0.lata dom
		mesh  Champ_front_xyz_debit_0/Champ_front_xyz_debit_0_PROFIL_VITX.son      MESH red
		width 10cm
		zoom2D -0.1715194150583894 5.134657491393821 -0.1087717572213625 2.064149271803881
	}
	figure { 
		title "PROFIL_VITX"
		include_description_curves 0
		labelX "Y AXIS"
		labelY "VITESSEX"
		width 10cm
		rangey 0.5 4.5
		curve {
			fonction 4
			style lines
			legend "User Ux=4"
		}
		curve {
			segment Champ_front_xyz_debit_0/Champ_front_xyz_debit_0 PROFIL_VITX 0
			style linespoints
			legend "t=0"
		}
		curve {
			segment Champ_front_xyz_debit_0/Champ_front_xyz_debit_0 PROFIL_VITX
			style points
			legend "t end"
		}
	}
	figure { 
		title "PROFIL_VITY"
		include_description_curves 0
		labelX "Y AXIS"
		labelY "VITESSEY"
		RangeY 0.2 1.2
		width 10cm
		curve {
			fonction 1
			style lines
			legend "User Uy=1"
		}
		curve {
			segment Champ_front_xyz_debit_0/Champ_front_xyz_debit_0 PROFIL_VITY 0
			style linespoints
			legend "t=0"
		}
		curve {
			segment Champ_front_xyz_debit_0/Champ_front_xyz_debit_0 PROFIL_VITY
			style points
			legend "t end"
		}
	}
}
