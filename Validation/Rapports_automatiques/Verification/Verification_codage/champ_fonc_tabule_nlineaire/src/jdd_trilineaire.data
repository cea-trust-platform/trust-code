# PARALLEL OK 8 #
dimension 3
pb_thermohydraulique pb
Domaine dom

# BEGIN MESH #
Mailler dom
{
    Pave Cavite
    {
        Origine 0 0 0
        Nombre_de_Noeuds 20 20 20
        Longueurs 1 1 1
    }
    {
        Bord paroi X = 0  0 <= Y <= 1  0 <= Z <= 1
        Bord paroi X = 1  0 <= Y <= 1  0 <= Z <= 1
        Bord paroi Y = 0  0 <= X <= 1  0 <= Z <= 1
        Bord paroi Y = 1  0 <= X <= 1  0 <= Z <= 1
        Bord paroi Z = 0  0 <= X <= 1  0 <= Y <= 1
        Bord paroi Z = 1  0 <= X <= 1  0 <= Y <= 1
    }
}
# END MESH #

# BEGIN PARTITION
Decouper dom
{
    partitionneur metis { nb_parts 2 }
    nb_parts_tot 2
    Larg_joint 2
    Nom_Zones dom
    ecrire_decoupage dom.txt
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter dom.Zones    dom
/* Read_file dom_ssz_par.geo ; */
END SCATTER #

Scheme_euler_implicit sch
Read sch
{
    tinit 0
    nb_pas_dt_max 0
    solveur implicite { solveur petsc cli { } }
}

VDF dis

fluide_incompressible  mil_fluide
lire mil_fluide
{
    rho      champ_uniforme 1  2000.0
    cp       champ_uniforme 1  3075.0
    beta_th  champ_fonc_tabule {
        pb viscosite_dynamique
        pb conductivite
        pb temperature
    }
    1
    {
        2 0 1 # viscosite #
        3 0 0.4 1 # conductivite #
        2 0 1 # temperature #

        # valeurs_v_c_t #
        0 # v = 0 c = 0   t = 0 #
        1 # v = 0 c = 0   t = 1 #
        2 # v = 0 c = 0.4 t = 0 #
        3 # v = 0 c = 0.4 t = 1 #
        0 # v = 0 c = 1   t = 0 #
        4 # v = 0 c = 1   t = 1 #
        5 6 7 8 9 10 # pareil pour v = 1 #
    }
    mu       champ_fonc_xyz dom 1 x
    lambda   champ_fonc_xyz dom 1 y
}

associate pb dom
associate pb mil_fluide

associate pb sch
discretize pb dis

Read pb
{
    navier_stokes_standard
    {
        solveur_pression petsc cholesky { }
        convection  { amont }
        diffusion   { }
        initial_conditions  { vitesse champ_uniforme 3 0 0 0 }
        boundary_conditions { paroi paroi_fixe }
    }

    convection_diffusion_temperature
    {
        convection  { amont }
        diffusion   { }
        initial_conditions  { temperature champ_fonc_xyz dom 1 z }
        boundary_conditions
        { paroi paroi_adiabatique }
    }

    Post_processing
    {
        format lata
        fields dt_post 1e8
        {
            viscosite_dynamique elem
            conductivite        elem
            dilatabilite        elem
            temperature         elem
        }
    }
}

Solve pb
End
