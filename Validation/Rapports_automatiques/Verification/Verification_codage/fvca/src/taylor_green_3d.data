# PARALLEL OK #
_lance_test_
dimension 3
Pb_hydraulique pb
Domaine dom

# BEGIN MESH #
Lire_MED { domain dom file mesh.med }
# END MESH #

# BEGIN PARTITION
Decouper dom
{
    partitionneur metis { Nb_parts 4 }
    nb_parts_tot 4
    Larg_joint 2
    zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

$dis  dis
Option_PolyMAC { use_osqp }

Schema_euler_implicite sch
Read sch
{
    tinit   0
    dt_max  0.5
    facsec 1e30
    seuil_statio 1e-8
    solveur $solv
}

Associate  pb dom
Associate  pb sch
Discretize pb dis

Read pb
{

    fluide_incompressible {
        mu  Champ_Uniforme 1 1
        rho Champ_Uniforme 1 1
    }


    Navier_Stokes_standard
    {
        methode_calcul_pression_initiale avec_sources_et_operateurs
        solveur_pression petsc cholesky { }
        convection { negligeable }
        diffusion  { }
        sources
        {
            source_qdm champ_fonc_xyz dom 3 -36*pi*pi*cos(2*pi*x)*sin(2*pi*y)*sin(2*pi*z) 0 0
        }
        initial_conditions
        {
            vitesse champ_fonc_xyz dom 3 0 0 0
        }
        boundary_conditions
        {
            boundary frontiere_ouverte_vitesse_imposee champ_front_fonc_xyz 3 -2*cos(2*pi*x)*sin(2*pi*y)*sin(2*pi*z) sin(2*pi*x)*cos(2*pi*y)*sin(2*pi*z) sin(2*pi*x)*sin(2*pi*y)*cos(2*pi*z)
        }
    }

    Post_processing
    {
        Definition_champs
        {
            p_moins_pmoy transformation
            {
                methode formule expression 1 p-pmoy
                sources
                {
                    interpolation { source refChamp { Pb_champ pb pression } localisation elem nom_source p } ,
                    reduction_0d { methode moyenne_ponderee source interpolation { source refChamp { Pb_champ pb pression } localisation elem } nom_source pmoy }
                }
                localisation elem
            }
            erreur_P transformation
            {
                methode formule expression 1 abs(pmp-sol)
                sources
                {
                    transformation
                    {
                        methode formule expression 1 p-pmoy
                        sources
                        {
                            interpolation { source refChamp { Pb_champ pb pression } localisation elem nom_source p } ,
                            reduction_0d { methode moyenne_ponderee source interpolation { source refChamp { Pb_champ pb pression } localisation elem } nom_source pmoy }
                        }
                        localisation elem
                        nom_source pmp
                    } ,
                    transformation { methode formule expression 1 -6*pi*sin(2*pi*x)*sin(2*pi*y)*sin(2*pi*z) localisation elem nom_source sol source refChamp { Pb_champ pb pression } }
                }
            }
            erreur_P_max reduction_0D { methode l2_norm sources_reference { erreur_P } }

            /* aux elems */
            erreur_V transformation
            {
                methode formule expression 1 abs(v-sol)
                sources
                {
                    Interpolation { localisation elem nom_source v source refChamp { Pb_champ pb vitesse } } ,
                    transformation { methode vecteur expression 3 -2*cos(2*pi*x)*sin(2*pi*y)*sin(2*pi*z) sin(2*pi*x)*cos(2*pi*y)*sin(2*pi*z) sin(2*pi*x)*sin(2*pi*y)*cos(2*pi*z) nom_source sol source refChamp { Pb_champ pb viscosite_dynamique } }
                }
                localisation elem
            }
            erreur_V_max reduction_0D { methode l2_norm source transformation { methode norme sources_reference { erreur_V } } }
        }
        probes
        {
            errP erreur_P_max periode 1e-6 Numero_elem_sur_maitre 0
            errV erreur_V_max periode 1e-6 Numero_elem_sur_maitre 0
        }
        Format Lata
        fields dt_post 1e10
        {
            p_moins_pmoy elem
            vitesse elem
            erreur_V elem
            erreur_P elem
        }
    }
}
Solve pb
lata_to_other lml NOM_DU_CAS NOM_DU_CAS
End
