Parameters {
	Title "verification ef_stab"
	Description "Calculs menes en VEF laminaire"
	Author "G.F."
	TestCase "test_ef_stab" "test.data"
	TestCase "test_ef_stab_ve" "test.data"
	TestCase "test_ef_stab_old" "test.data"
}

Chapter {
	Title "Profil de vitesse"
	Description "Comparaison du profil de vitesse au profil de Poiseuille.  Voir data... "
Figure {
	Title "Trace du profil de u final "
	Dimension 2
	LabelX "m"
	LabelY "m/s"
	Curve {
		legend "EF_stab { } "
		origin "Trio_U"
		Version "1.5.3-beta_build_180907"
		file ./test_ef_stab/Moyennes_spatiales_vitesse_rho_mu_fin
	Style linespoints
		# TypeLigne 4
		# TypePoints 4
	}
	Curve {
		legend "EF_stab { volumes_etendues }"
		origin "Trio_U"
		Version "1.5.3-beta_build_180907"
		file ./test_ef_stab_ve/Moyennes_spatiales_vitesse_rho_mu_fin
	Style linespoints
		# TypeLigne 4
		# TypePoints 4
	}
	Curve {
		legend "EF_stab { old }"
		origin "Trio_U"
		Version "1.5.3-beta_build_180907"
		file ./test_ef_stab_old/Moyennes_spatiales_vitesse_rho_mu_fin
	Style linespoints
		# TypeLigne 4
		# TypePoints 4
	}
	Curve {
	        legend "analytique"
		origin "Trio_U"
		Version "1.5.3-beta_build_180907"
		file ./test_ef_stab/Moyennes_spatiales_vitesse_rho_mu_fin
                 columns ($1) (1.5*($1)*(2-$1)) 
		Style linespoints
		# TypeLigne 2
		# TypePoints 2
	}
}

