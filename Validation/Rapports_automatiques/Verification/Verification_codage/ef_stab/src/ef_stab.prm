	Dimension 2
#	RangeX 0. 2.5
	LabelX "s"
	RangeY -0.1 0.2
	LabelY ""
	Curve {
		legend "ef_stab { }"
		origin "Trio_U"
		Version "1.5.3-beta_build_180907"
		file test_ef_stab/test_Pressure_Gradient_pb_periox	

		Style lines
		# TypeLigne 4
		# TypePoints 4
	}
	Curve {
		legend "ef_stab { volumes_etendus } "
		origin "Trio_U"
		Version "1.5.3-beta_build_180907"
		file test_ef_stab_ve/test_Pressure_Gradient_pb_periox	

		Style lines
		# TypeLigne 4
		# TypePoints 4
	}
	Curve {
