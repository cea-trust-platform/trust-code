Chapter {
	Title Convergence en maillage en vitesse 
	Figure {
	labelx N
	labely erreur max 
	logx
	logy
	Include_Description_Curves 0
	width 12cm
	Title "convergence de la convection vitesse en maillage "
	 
		Curve {
		legend amont
		file conv_glob_amont_qdm
		  style linespoints
		}
		 
		Curve {
		legend muscl
		file conv_glob_muscl_qdm
		  style linespoints
		}
		 
		Curve {
		legend ef_stab
		file conv_glob_ef_stab_qdm
		  style linespoints
		}
		 
		Curve {
		legend amont_old
		file conv_glob_amont_old_qdm
		  style linespoints
		}
		 
		Curve {
		legend centre
		file conv_glob_centre_qdm
		  style linespoints
		}
		 
		Curve {
		legend N^-1 
		fonction 1./x
		  style lines
		}
		Curve {
		legend N^-2 
		fonction 1./x/x
		  style lines
		}
	}			
} 
