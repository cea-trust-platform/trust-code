# PARALLEL OK #
dimension 2
Pb_hydraulique pb
Domaine dom

# BEGIN MESH #
Lire_med family_names_from_group_names dom mesh mesh.med
# END MESH #

# BEGIN PARTITION
Postraiter_domaine
{
    fichier mesh.lata
    format lata
    domaine dom
}

Partition dom
{
    Partition_tool tranche { tranches 2 1 }
    larg_joint 2
    zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

CoviMAC  dis

Schema_euler_implicite sch
Read sch
{
    tinit 0
    dt_max 1
    seuil_statio 1e-8
    # solveur solveur_U_P { solveur petsc cholesky { cli { -mat_mumps_icntl_23 2000 } } } #
    solveur implicite
    {
        solveur petsc cli { }
    }
}


Fluide_Incompressible eau
Read eau
{
    mu  Champ_Uniforme 1 2.5
    rho Champ_Uniforme 1 1000
}

Associate  pb dom
Associate  pb sch
Associate  pb eau
Discretize pb dis

Read pb
{
    Navier_Stokes_standard
    {
        solveur_pression petsc cholesky { }
        convection { negligeable }
        diffusion  { }
        initial_conditions { vitesse Champ_Uniforme 2 0 0 }
        boundary_conditions
        {
            up    paroi_defilante Champ_Front_Uniforme 2 1 0
            down  paroi_fixe
            left  frontiere_ouverte_pression_imposee Champ_Front_Uniforme 1 200
            right frontiere_ouverte_pression_imposee Champ_Front_Uniforme 1 100
        }
    }

    Post_processing
    {
        Definition_champs
        {
            erreur_P transformation
            {
                methode formule expression 1 abs(p-sol)
                sources
                {
                    refChamp { Pb_champ pb pression_pa nom_source p } ,
                    transformation { methode formule expression 1 200-100*x localisation elem nom_source sol source refChamp { Pb_champ pb pression } }
                }
            }
            erreur_P_max reduction_0D { methode max sources_reference { erreur_P } }

            /* aux elems */
            erreur_V transformation
            {
                methode formule expression 1 v-sol
                sources
                {
                    Interpolation { localisation elem nom_source v source refChamp { Pb_champ pb vitesse } } ,
                    transformation { methode vecteur expression 2 1/2.5*100*y*(1-y)/2+y 0 nom_source sol source refChamp { Pb_champ pb pression } }
                }
            }
            /* aux faces
            erreur_V transformation
            {
                methode formule expression 1 v-sol
                sources
                {
                    refChamp { Pb_champ pb vitesse nom_source v } ,
                    transformation { methode formule expression 2 1/2.5*100*y*(1-y)/2+y 0 nom_source sol source refChamp { Pb_champ pb vitesse } }
                }
            } */
            erreur_V_max reduction_0D { methode max source transformation { methode norme sources_reference { erreur_V } } }

        }
        probes
        {
            errP erreur_P_max periode 1e-6 Numero_elem_sur_maitre 0
            errV erreur_V_max periode 1e-6 Numero_elem_sur_maitre 0
        }

        Format Lata
        fields dt_post 10
        {
            pression_pa elem
            vitesse elem
            vitesse faces
            erreur_V
            erreur_P
        }
    }
}
Solve pb
lata_to_other lml NOM_DU_CAS NOM_DU_CAS
End
