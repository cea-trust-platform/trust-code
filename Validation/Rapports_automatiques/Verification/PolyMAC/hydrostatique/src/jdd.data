# Hydraulique 2D laminaire avec "trou" dans le maillage #
# PARALLEL OK #
dimension 2
Pb_hydraulique pb
Domaine dom

# BEGIN MESH #
Lire_med family_names_from_group_names dom mesh mesh.med
# END MESH #

# BEGIN PARTITION
/*
Postraiter_domaine
{
    fichier mesh.lata
    format lata
    domaine dom
}
*/

Partition dom
{
    Partition_tool tranche { tranches 2 1 }
    larg_joint 2
    zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

PolyMAC  dis

Schema_euler_implicite sch
Read sch
{
    tinit 0
    tmax 5
    dt_max 0.1
    facsec 1e30
    solveur implicite
    {
        solveur petsc cli
        {
            -pc_type fieldsplit
            -pc_fieldsplit_0_field b1
            -pc_fieldsplit_1_field b0
            -pc_fieldsplit_type schur
            -ksp_type fgmres
            -ksp_max_it 10
        }
    }
}

Fluide_Incompressible eau
Read eau
{
    mu  Champ_Uniforme 1 3.7e-05
    rho Champ_Uniforme 1 2
}

Associate  pb dom
Associate  pb sch
Associate  pb eau
Discretize pb dis

Read pb
{
    Navier_Stokes_standard
    {
        solveur_pression petsc cholesky { }
        convection { negligeable }
        diffusion  { negligeable }
        sources    { source_qdm champ_uniforme 2 0 -10 }
        initial_conditions { vitesse Champ_Uniforme 2 0 0 }
        boundary_conditions
        {
            up    frontiere_ouverte_pression_imposee Champ_Front_Uniforme 1 1e5
            down  paroi_fixe
            left  paroi_fixe
            right paroi_fixe
        }
    }

    Post_processing
    {
        Definition_champs
        {
            erreur_P transformation
            {
                methode formule expression 1 abs(p-sol)
                sources
                {
                    refChamp { Pb_champ pb pression nom_source p } ,
                    transformation { methode formule expression 1 1e5/2+10*(1-y) localisation elem nom_source sol source refChamp { Pb_champ pb pression } }
                }
            }
            erreur_P_max reduction_0D { methode max sources_reference { erreur_P } }
            erreur_V_max
                    reduction_0D
                    {
                        methode max source transformation
                        {
                            methode norme source interpolation { domaine dom localisation elem source refchamp { Pb_champ pb vitesse } }
                        }
                    }

        }
        probes
        {
            errP erreur_P_max periode 1e-6 Numero_elem_sur_maitre 0
            errV erreur_V_max periode 1e-6 Numero_elem_sur_maitre 0
        }

        Format Lata
        fields dt_post 1.3
        {
            erreur_P elem
            pression elem
            pression_pa elem
            vitesse elem
        }
    }
    sauvegarde formatte ascci.sauv
}
Solve pb
lata_to_other lml NOM_DU_CAS NOM_DU_CAS
End
